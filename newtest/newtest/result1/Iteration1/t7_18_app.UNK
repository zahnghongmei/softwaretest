oAndFolder(item)\n        if (this.repoType == 'article') {\n          this.getArticleRepoAndFolderinfo(item)\n          this.$store.commit('SET_REPOFOLDERBREAD_NAME', '')\n          this.$root.eventHub.$emit('getFolderInfo', '', '')\n        }\n        if (this.repoType == 'material') {\n          this.getMaterialRepoAndFolderInfo(item)\n          $('.each_operate').removeClass('each_operateActive')\n          this.choiceDefaultFolder(item)\n\n        }\n\n      }\n\n      if (this.repoType == 'material' && item.hasOwnProperty('flag_sys')) {\n        this.$root.eventHub.$emit('setToPic', '')\n        this.$store.commit('SET_MATERIAL_TYPE', 'pic')\n        this.$root.eventHub.$emit('getPicType', 'single')\n      }\n\n    },\n\n    deleteFile(item) {\n      this.$store.commit('SET_DELETE_TYPE', 'deleteFolder')\n      this.$root.eventHub.$emit('setDeteModel', item)\n    },\n    addFolder() {\n      if (this.addFolderInfo == '') {\n        toastr.error('请选择库或者文件夹')\n      } else {\n        if (this.addFolderInfo.hasOwnProperty('flag_sys') && this.addFolderInfo.flag_sys == true) {\n          this.$root.eventHub.$emit('setAddobjModel', '')\n        } else if (this.addFolderInfo.hasOwnProperty('folder_name') && this.entityMap[this.addFolderInfo.id_repo].flag_sys == true) {\n          this.$root.eventHub.$emit('setAddobjModel', '')\n        } else {\n          toastr.warning('无权限')\n        }\n\n      }\n\n    },\n    getArticleRepoAndFolderinfo(item) {\n      if (item.role_item.flag_create) {\n        this.$store.commit('SET_ISSHOWEDITOR_ADDICON', true)\n      } else {\n        this.$store.commit('SET_ISSHOWEDITOR_ADDICON', false)\n      }\n      //获取文章总数和今日更新\n      var sendCurrentData = {\n        \"userId\": this.userId,\n        \"token\": this.token,\n        \"seed\": this.seed,\n        'orgId': this.orgId,\n        \"params\": {\n          \"api_name\": this.whichName,\n          \"id\": item.id\n        }\n      }\n\n      this.$store.dispatch('GET_ARTICLE_ALLTOT