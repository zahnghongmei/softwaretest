ass=\"sure_cancle\"\n               @click.stop='sureDelete()'>确&nbsp;&nbsp;定</div>\n          <div class=\"sure_cancle cancle_operate\"\n               v-on:click=\"closeDelete()\">取&nbsp;&nbsp;消</div>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport * as api from '../../api'\n//var debounce = require('throttle-debounce/debounce')\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport Vue from 'vue'\nexport default {\n\n  components: {\n\n  },\n\n  created() {\n\n\n  },\n\n  mounted() {\n    this.$root.eventHub.$on('setDeteModel', (obj) => {\n      this.show = true\n      this.deleteObj = obj\n    })\n\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setDeteModel')\n  },\n\n  data() {\n    return {\n      show: false,\n      deleteObj: ''\n    }\n  },\n\n  updated() {\n\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    entityMap() {\n      return this.$store.state.repo.entityMap\n    },\n    repoData() {\n      return this.$store.state.repo.modelManagerDataList\n    },\n    editorFoldId() {\n      return this.$store.state.articlelist.editorFoldId\n    },\n    deleteType() {\n      return this.$store.state.articlelist.deleteType\n    },\n    articleDetail() {\n      return this.$store.state.articledetail.articleDetail\n    },\n    downWhichArticleLsit() {\n      return this.$store.state.articlelist.downWhichArticleLsit\n    },\n\n\n\n  },\n\n  methods: {\n    closeDelete() {\n      this.show = false\n    },\n    sureDelete() {\n      if (this.deleteType == 'deleteFolder') {\n        var option = {\n          \"userId\": this.userId,         // 必须字段：  用户 id，必须字段\n          \"token\": this.token,          // 必