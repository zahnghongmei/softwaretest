<transition name=\"fade\">\n    <div class=\"modal\"\n         v-show=\"show\">\n      <div class=\"container\">\n        <div class=\"container_top\">\n          <span class=\"container_top_title\">另存为</span>\n          <span class=\"iconfont icon-close\"\n                v-on:click=\"closeModal()\"></span>\n        </div>\n        <div class=\"share_list\">\n          <div class=\"share_list_left\">\n            <div class=\"share_list_title\"\n                 v-if=\"saveAsType == 'manuscript'\">稿件库</div>\n            <div class=\"share_list_title\"\n                 v-if=\"saveAsType == 'picture'\">素材库</div>\n            <ul class=\"share_list_content\">\n              <repobar :models='repoFolderData'></repobar>\n            </ul>\n          </div>\n          <div class=\"share_list_left share_list_right\">\n            <div class=\"share_list_title\">\n              <span>已选择文件夹</span>\n              <span style=\"color:#51BD90;\">{{hasChoiceUser.length}}</span>\n              <span>个</span>\n            </div>\n            <ul class=\"share_list_content\">\n              <li v-for='(item,index) in hasChoiceUser'>\n                <div class=\"share_each\">\n                  <span class=\"sharch_name\"\n                        :title='item.folder_name'>{{item.folder_name | substr}}</span>\n                  <div>\n                    <div class=\"share_clear\">\n                      <span class=\"iconfont icon-close\"\n                            @click.stop='deleteChoiceUser(item,index)'></span>\n                    </div>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n        <div class=\"share_button\">\n          <div class=\"sure_cancle\"\n               @click.stop='sureSaveAs()'\n               v-show='!isuploading'>确&nbsp;&nbsp;定</div>\n          <div class='sure_cancle'\n               style='background:#999999;cursor:not-allowed;'\n               v-show='isuploading'>确&nbsp;&nbsp;定</div>\n\n          <div class=\"sure_cancle cancle_operate\"\n               @click.stop='closeModal()'>取&nbsp;&nbsp;消</div>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport * as api from '../../api'\n// var debounce = require('throttle-debounce/debounce')\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport repobar from './repobar.vue'\nimport Vue from 'vue'\nexport default {\n\n  components: {\n    repobar\n  },\n\n  created() {\n\n  },\n\n  mounted() {\n    this.$root.eventHub.$on('setMSaveAs', (obj, type) => {\n      this.show = true\n      this.saveAsObj = obj\n      this.saveAsType = type\n    })\n    this.$root.eventHub.$on('addUser', (obj) => {\n      this.hasChoiceUser.push(obj)\n    })\n    this.$root.eventHub.$on('deleteUser', (obj) => {\n      for (var i = 0; i < this.hasChoiceUser.length; i++) {\n        if (this.hasChoiceUser[i].id == obj.id) {\n          this.hasChoiceUser.splice(i, 1);\n          break;\n        }\n      }\n    })\n\n    niceScroller('.share_list_content')\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setMSaveAs')\n    this.$root.eventHub.$off('addUser')\n    this.$root.eventHub.$off('deleteUser')\n  },\n\n  data() {\n    return {\n      show: false,\n      saveAsObj: '',\n      hasChoiceUser: [],\n      saveAsType: '',\n      isuploading: false\n    }\n  },\n\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller('.share_list_content')\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    shareType() {\n      return this.$store.state.articlelist.shareType\n    },\n    repoFolderData() {\n      return this.$store.state.repo.repoFolderData\n    }\n\n  },\n\n  methods: {\n    closeModal() {\n      this.show = false\n    },\n\n    deleteChoiceUser(item, index) {\n      this.hasChoiceUser.splice(index, 1)\n      this.$root.eventHub.$emit('clearStyle', item, this.saveAsType)\n    },\n    sureSaveAs() {\n      var arr = []\n      for (var i = 0; i < this.hasChoiceUser.length; i++) {\n        arr.push(this.hasChoiceUser[i].id)\n      }\n      var option = {\n        \"userId\": this.userId,              // 必须字段：用户 id\n        \"token\": this.token,                 // 必须字段： token\n        \"seed\": this.seed,                   // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": {                        // 必须字段： 文章参数\n          \"id\": this.saveAsObj.id,            // 必须字段： 文章 ID\n          \"folder_ids\": arr\n        }\n      }\n      if (this.hasChoiceUser.length > 0) {\n        this.isuploading = true\n        if (this.saveAsType == 'manuscript') {\n          api._post('article/save_article_new', option).then((res) => {\n\n            if (res.data && res.data.code && res.data.code == 200) {\n              toastr.success('另存稿件成功')\n              this.show = false\n            } else {\n              if (res.data && res.data.message) {\n                toastr.warning(res.data.message)\n              }\n            }\n\n          }).catch((error) => {\n            console.error(error)\n          })\n        } else if (this.saveAsType == 'picture') {\n          api._post('material/picture/save_as_new', option).then((res) => {\n            if (res.data && res.data.code && res.data.code == 200) {\n              toastr.success('另存图片成功')\n              this.show = false\n            } else {\n              if (res.data && res.data.message) {\n                toastr.warning(res.data.message)\n              }\n            }\n\n          }).catch((error) => {\n            console.error(error)\n          })\n        }\n\n      } else {\n        toastr.warning('请至少选择一个文件夹')\n      }\n    }\n\n  },\n\n  watch: {\n    show() {\n      if (this.show == false) {\n        this.$root.eventHub.$emit('cleardata', '')\n        this.hasChoiceUser = []\n        this.saveAsObj = '',\n          this.saveAsType = ''\n        this.isuploading = false\n      }\n    }\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n@import \"../../static/css/modal\";\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nul,\nli {\n  list-style: none;\n}\n.expand-transition {\n  transition: all 1s ease;\n}\n.expand-enter,\n.expand-leave {\n  opacity: 0;\n}\n.expand-enter .container,\n.expand-leave .container {\n  /*margin-top: -800px;*/\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n\n.container {\n  transition: all 1s ease;\n  width: 530px;\n  height: 560px;\n  position: absolute;\n  left: 40%;\n  top: 20%;\n  background-color: #ffffff;\n  box-shadow: 0px 0px 5px #999;\n  border-radius: 5px;\n  z-index: 999;\n  display: flex;\n  flex-direction: column;\n}\n\n.container_top {\n  width: 100%;\n  height: 44px;\n  background: #f7faff;\n  position: relative;\n  padding-left: 20px;\n  padding-right: 20px;\n  clear: both;\n  line-height: 44px;\n  border-bottom: 1px solid #c0beff;\n  margin-bottom: 10px;\n}\n.container_top .iconfont {\n  float: right;\n  font-size: 26px;\n  cursor: pointer;\n  color: #5f7094;\n}\n.container_top .container_top_title {\n  color: #333333;\n  font-size: 14px;\n  font-family: \"微软雅黑\";\n  float: left;\n}\n\n.share_list {\n  width: 100%;\n  flex: 1;\n  padding-left: 20px;\n  padding-right: 20px;\n  display: flex;\n}\n.share_list_left {\n  flex: 1;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  border: 1px solid #c8c8c8;\n}\n.share_list_right {\n  margin-left: 20px;\n}\n.share_list_title {\n  width: 100%;\n  height: 44px;\n  line-height: 44px;\n  color: #333333;\n  font-size: 12px;\n  font-family: \"微软雅黑\";\n  padding-left: 10px;\n  border-bottom: 1px solid #c8c8c8;\n  margin-bottom: 10px;\n}\n.share_list_content {\n  width: 100%;\n  height: 360px;\n  overflow: hidden;\n}\n.share_list_content .share_each {\n  width: 100%;\n  height: 34px;\n  line-height: 34px;\n  padding-left: 10px;\n  position: relative;\n}\n.share_each .iconfont {\n  color: #b5caec;\n  font-size: 14px;\n  cursor: pointer;\n}\n.share_each .sharch_name {\n  color: #666666;\n  font-size: 12px;\n}\n\n.share_each img {\n  width: 20px;\n  height: 20px;\n  border-radius: 10px;\n  position: absolute;\n  left: 26px;\n  top: 7px;\n}\n\n.sharch_name_second {\n  padding-left: 44px;\n}\n\n.share_choice {\n  position: absolute;\n  right: 20px;\n  top: 8px;\n  width: 16px;\n  height: 16px;\n  border: 1px solid #cccccc;\n  border-radius: 8px;\n  cursor: pointer;\n  display: block;\n}\n.share_haschoice {\n  position: absolute;\n  right: 20px;\n  top: 8px;\n  width: 16px;\n  height: 16px;\n  background: #51bd90;\n  border-radius: 8px;\n  cursor: pointer;\n  text-align: center;\n  line-height: 16px;\n  display: none;\n}\n.share_haschoice .iconfont {\n  color: #fff;\n  font-size: 12px;\n}\n\n.share_clear {\n  position: absolute;\n  right: 20px;\n  top: 8px;\n  width: 16px;\n  height: 16px;\n  background: #f86c7d;\n  border-radius: 8px;\n  cursor: pointer;\n  text-align: center;\n  line-height: 16px;\n}\n.share_clear .iconfont {\n  font-size: 12px;\n  color: #fff;\n}\n\n.share_button {\n  width: 100%;\n  height: 76px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n.sure_cancle {\n  width: 110px;\n  height: 36px;\n  border-radius: 4px;\n  background: #716bff;\n  line-height: 36px;\n  text-align: center;\n  color: #fff;\n  font-size: 14px;\n  cursor: pointer;\n}\n.cancle_operate {\n  background: #fff;\n  color: #666666;\n  border: 1px solid #716bff;\n  margin-left: 20px;\n}\n</style>\n"],sourceRoot:""}])},Z77O:function(n,t){n.exports="data:image/png;base64,"},ZS5A:function(n,t){n.exports="data:image/png;base64,"},ZWdd:function(n,t,e){var i=e("ZwAp");"string"==typeof i&&(i=[[n.i,i,""]]),i.l