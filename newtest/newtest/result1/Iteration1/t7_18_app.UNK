\n  cursor: pointer;\n}\n\n.close:hover {\n  background-color: #77A7FF;\n}","<template>\n  <transition name=\"fades\">\n    <div class=\"modal\"\n         v-if=\"visible\"\n         id='guanshan_app_modal_id'>\n      <div class=\"share_box\">\n        <div class=\"share_box_title\">\n          <span>推送服务</span>\n          <span class=\"el-icon-close icon-cuo\"\n                @click='closeModal'></span>\n        </div>\n        <iframe :src=\"iframeUrl\"\n                frameborder=\"0\"></iframe>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\n\nimport * as api from '../../api'\n\nexport default {\n  components: {\n\n  },\n\n  data() {\n    return {\n      visible: false,\n      currentArticleData: '',\n      iframeUrl: '',\n      toasttStatus: false,\n      pushIndex: 0\n    }\n  },\n\n  mounted() {\n    this.receiveMessage()\n    this.$root.eventHub.$on('setPushPcInfo', (data) => {\n      this.visible = true\n      this.currentArticleData = data\n      this.iframeUrl = api.iframeGuanshanUrl + '#/pcpush' + '?userId=' + this.userId + '&token=' + this.token + '&seed=' + this.seed + '&orgId=' + this.orgId + '&id_content=' + data.id + '&title=' + data.title + '&type=' + this.repoType + '#/'\n\n    })\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setPushPcInfo')\n  },\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    repoType() {\n      return this.$store.state.repo.repoType\n    }\n\n  },\n  methods: {\n    closeModal() {\n      this.visible = false\n      this.iframeUrl = ''\n    },\n    receiveMessage() {\n      var self = this\n      var receiveMessageInfo = function (e) {\n        if (self.pushIndex == 0) {\n          if (e.data == 'pcpushsuccess') {\n            //toastr.success('推送成功'