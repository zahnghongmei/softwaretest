ler\'\nimport commentlog from \'../../mixins/commentlog.js\'\nimport { loadingTranstion, closeLoading } from \'../../helpers/loading.js\'\nexport default {\n  mixins: [commentlog],\n  components: {\n    comments\n  },\n\n  created() {\n    if (this.currentAuditArticle == \'\' && this.currentAuditArticleStatus == \'\') {\n      var msgoption = this.getUrlCode()\n      if (msgoption.token && msgoption.msg_type && msgoption.id_item && msgoption.id_audit) {\n        var content_type = \'\'\n        if (msgoption.msg_type == \'audit_article_request\' || msgoption.msg_type == \'audit_article_status\') {\n          content_type = \'article\'\n        }\n        if (msgoption.msg_type == \'audit_video_request\' || msgoption.msg_type == \'audit_video_status\') {\n          content_type = \'video\'\n          this.$store.commit(\'SET_MATERIAL_TYPE\', \'audit\')\n        }\n        if (msgoption.msg_type == \'audit_article_request\' || msgoption.msg_type == \'audit_video_request\') {\n          this.$store.commit(\'SET_AUDITARTICLE_SHOW\', true)\n        }\n        if (msgoption.msg_type == \'audit_article_status \' || msgoption.msg_type == \'audit_video_status\') {\n          this.$store.commit(\'SET_AUDITARTICLE_SHOW\', false)\n        }\n        var getDataOption = {\n          token: msgoption.token,\n          content_type: content_type,\n          id_content: msgoption.id_item,\n          id: msgoption.id_audit\n        }\n        this.getCodeAndRepo(getDataOption)\n        this.getAuditDetaailForRouter(msgoption.id_audit)\n\n      } else {\n        this.getRepo()\n        this.getMaterialData()\n        //this.getWaterMark()\n        var localRepoObj = localStorage.getItem("currentAuditDataInfo")\n        if (localRepoObj) {\n          var currentAuditDataObj = JSON.parse(localRepoObj)\n          this.$store.commit(\'SET_CURRENTAUDIT_ARTICLEDATA\', currentAuditDataObj)\n          this.getcontentinfo(currentAuditDataObj)\n          if (currentAuditDataObj.content_type == \'video\') {\n            this.$store.commit(\'SET_MATERIAL_TYPE\', \'audit\')\n          }\n\n          this.$store.commit(\'SET_COMMENTS_ID\', currentAuditDataObj.id_content)\n          var auditArticleTypeBoolean = localStorage.getItem("auditArticleType")\n          if (auditArticleTypeBoolean == \'false\') {\n            this.$store.commit(\'SET_AUDITARTICLE_SHOW\', false)\n          } else {\n            this.$store.commit(\'SET_AUDITARTICLE_SHOW\', true)\n          }\n\n        }\n      }\n\n\n\n\n\n    }\n\n    window.addEventListener("beforeunload", () => {\n      if (this.$store.state.page.currentAuditArticle) {\n        localStorage.setItem("currentAuditDataInfo", JSON.stringify(this.$store.state.page.currentAuditArticle))\n      }\n      localStorage.setItem("auditArticleType", this.$store.state.auditarticle.auditArticleType)\n    })\n\n\n\n  },\n  updated() {\n\n  },\n\n  mounted() {\n\n  },\n\n  destroyed() {\n\n  },\n\n  data() {\n    return {\n      showComments: false,\n      auditAdviceText: \'\',\n      isLoaded: false\n    }\n  },\n\n  updated() {\n\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    auditArticleType() {\n      return this.$store.state.auditarticle.auditArticleType\n    },\n    commentsData() {\n      return this.$store.state.articledetail.commentsData\n    },\n    currentAuditArticle() {\n      return this.$store.state.page.currentAuditArticle\n    },\n    auditAdviceDataList() {\n      return this.$store.state.page.auditAdviceDataList\n    },\n    currentMaterialType() {\n      return this.$store.state.page.currentMaterialType\n    },\n    auditEntityContent() {\n      return this.$store.state.page.auditEntityContent\n    },\n    currentAuditArticleStatus() {\n      return this.$store.state.articledetail.currentAuditArticleStatus\n    },\n    showIsVideo() {\n      if (this.currentAuditArticle.content_type == "video") {\n        return false\n      } else {\n        return true\n      }\n    }\n\n\n  },\n\n  methods: {\n    getCodeAndRepo(msgoption) {\n      if (msgoption.token) {\n        api._get(\'user/get_user_info\', \'\').then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            if (res.data.data.userId) {\n              locCookie.setCookie(\'userId\', res.data.data.userId)\n            } else {\n              locCookie.setCookie(\'userId\', \'\')\n            }\n            if (res.data.data.seed) {\n              locCookie.setCookie(\'seed\', res.data.data.seed)\n            } else {\n              locCookie.setCookie(\'seed\', \'\')\n            }\n\n            if (res.data.data.user.user_name) {\n              locCookie.setCookie(\'username\', res.data.data.user.user_name)\n            } else {\n              locCookie.setCookie(\'username\', \'\')\n            }\n            if (res.data.data.user.head_img) {\n              locCookie.setCookie(\'headimg\', res.data.data.user.head_img)\n            } else {\n              locCookie.setCookie(\'headimg\', \'\')\n            }\n            if (res.data.data.orgId) {\n              locCookie.setCookie(\'orgId\', res.data.data.orgId)\n            } else {\n              locCookie.setCookie(\'orgId\', \'\')\n            }\n            if (res.data.data.token) {\n              locCookie.setCookie(\'token\', res.data.data.token)\n            } else {\n              locCookie.setCookie(\'token\', \'\')\n            }\n            if (res.data.data.orgRoot.org_name) {\n              locCookie.setCookie(\'orgName\', res.data.data.orgRoot.org_name)\n            } else {\n              locCookie.setCookie(\'orgName\', \'\')\n            }\n\n\n            var option = {\n              token: res.data.data.token,\n              userId: res.data.data.userId,\n              seed: res.data.data.seed,\n              orgId: res.data.data.orgId,\n              username: res.data.data.user.user_name,\n              headimg: res.data.data.user.head_img,\n              orgName: res.data.data.orgRoot.org_name\n            }\n            this.$store.commit(\'SET_TOKEN_DATA\', option)\n            this.$store.commit(\'SET_COMMENTS_ID\', msgoption.id_content)\n            if (this.token) {\n              this.getRepo()\n              this.getMaterialData()\n              //this.getWaterMark()\n              this.getcontentinfo(msgoption)\n            }\n\n\n          } else {\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          console.error(error)\n        })\n      }\n\n    },\n\n    //获取审核详情（不用保存数据 只是为了当数据不存咋的时候跳转）\n    getAuditDetaailForRouter(id) {\n      var auditoptions = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "orgId": this.orgId,               // 必须字段：公司 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        "params": {                       // 必须字段： 参数\n          "id": id\n        }\n      }\n      api._post(\'content_audit/audit_detail\', auditoptions).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          console.log(\'有数据\')\n\n        } else {\n          this.$router.replace(\'/collectandsend/collecentry\')\n        }\n      }).catch((error) => {\n        console.log(error)\n      })\n\n    },\n\n\n    getParam(name, url) {\n      if (!url) url = location.href;\n      name = name.replace(/[\\[]/, "\\\\\\[").replace(/[\\]]/, "\\\\\\]");\n      var regexS = "[\\\\?&]" + name + "=([^&#]*)";\n      var regex = new RegExp(regexS);\n      var results = regex.exec(url);\n      return results == null ? null : results[1];\n    },\n\n    getUrlCode() {\n      let currentUrl = window.location.href\n      let token = this.getParam(\'token\', currentUrl)\n      let msg_type = this.getParam(\'msg_type\', currentUrl)\n      let id_item = this.getParam(\'id_item\', currentUrl)\n      let id_audit = this.getParam(\'id_audit\', currentUrl)\n      if (token) {\n        localStorage.setItem(\'token\', token)\n      }\n      var option = {\n        token: token,\n        msg_type: msg_type,\n        id_item: id_item,\n        id_audit: id_audit\n      }\n      return option\n    },\n    getRepo() {\n      this.$store.commit(\'SET_CLEAR_REPODATA\')\n      var option = {\n        "userId": this.userId,  // 必须字段：  用户 id，必须字段\n        "token": this.token,    // 必须字段：  token\n        "seed": this.seed,      // 必须字段：  seed\n        \'orgId\': this.orgId,\n        "params": {          // 必须字段：  参数\n          "flag_type": \'article\',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch(\'GET_REPOFODLER_DATA\', option)\n    },\n\n    getMaterialData() {\n      var option = {\n        "userId": this.userId,  // 必须字段：  用户 id，必须字段\n        "token": this.token,    // 必须字段：  token\n        "seed": this.seed,      // 必须字段：  seed\n        \'orgId\': this.orgId,\n        "params": {          // 必须字段：  参数\n          "flag_type": \'material\',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch(\'GET_MATERIAL_DATA\', option)\n    },\n\n    //获取水印列表\n    getWaterMark() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段： 参数\n        }\n      }\n      this.$store.dispatch(\'GET_WATERMARKLIST_DATA\', option)\n\n    },\n\n    goToIndex() {\n      this.$router.replace(\'/collectandsend/collecentry\')\n      this.$store.commit(\'SET_CLEARAUDITENTITY_CONTENTDATA\')\n      this.$store.commit(\'SET_CURRENTAUDIT_ARTICLEDATA\', \'\')\n      this.$store.commit(\'SET_CURRENTAUDIT_ARTICLESTATUSDATA\', \'\')\n      localStorage.setItem("currentAuditDataInfo", \'\')\n      localStorage.setItem("auditArticleType", \'\')\n      $("#jquery_jplayer_1").jPlayer("destroy")\n      this.$store.commit(\'SET_ISPOSITIONMYSHARE_STATUS\', false)\n    },\n    showOrHideComments() {\n      this.showComments = !this.showComments\n    },\n    closeOpacity() {\n      this.showComments = false\n    },\n    getNextAuditPerson() {\n      this.$root.eventHub.$emit(\'setAuditArticleModal\', this.currentAuditArticle, \'choiceNextAuditPerson\', this.auditAdviceText)\n    },\n    auditNoThrough() {\n      this.auditOperate(\'no\')\n    },\n    auditHasThrough() {\n      this.auditOperate(\'yes\')\n    },\n    auditOperate(status) {\n      var option = {\n        "userId": this.userId,                // 必须字段：用户 id\n        "orgId": this.orgId,                  // 必须字段：公司 id\n        "token": this.token,                   // 必须字段： token\n        "seed": this.seed,                     // 必须字段： seed\n        "params": {                          // 必须字段： 参数\n          "id": this.currentAuditArticle.id,               // 必须字段：审核条目的 ID\n          "audit_status": status,  // 必须字段：审核通过为  yes  审核不通过为  no   审核通过并指定下一个审核人为  doing\n          "id_to_user": \'\',  // 关联的可选（必须）字段：下一步审核人的 ID。当 audit_status 的值为 doing 时，此字段必须填。\n          "comment": this.auditAdviceText            // 可选字段：审核的评论内容\n        }\n      }\n      loadingTranstion()\n      api._post(\'content_audit/do_audit\', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          toastr.success(\'提交成功\')\n          this.$router.replace(\'/collectandsend/collecentry\')\n          this.auditAdviceText = \'\'\n          closeLoading()\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n          closeLoading()\n        }\n\n      }).catch((error) => {\n        console.error(error)\n        closeLoading()\n      })\n    },\n    simulatePush() {\n      //this.$root.eventHub.$emit(\'setSimulationPushModel\', \'\')\n      //this.$root.eventHub.$emit(\'setPushModel\', this.currentAuditArticle.content)\n      this.$root.eventHub.$emit(\'setPushService\', this.currentAuditArticle.content)\n    },\n    editorCurrentArticle() {\n      if (this.currentAuditArticle.content != \'\') {\n        this.$store.commit(\'SET_EDITORDATA_ITEM\', this.currentAuditArticle.content)\n        this.$store.commit(\'SET_SAVE_CURRENTROUTER\', this.$route.path)\n        this.$router.replace(\'/editormain/editorcontent\')\n        this.$store.commit(\'SET_EDITORBUTTON_ROLE\', false)\n      } else {\n        toastr.warning(\'文章不能为空\')\n      }\n    }\n\n\n  },\n\n  watch: {\n    auditEntityContent() {\n      this.$nextTick(() => {\n        playVideo(this.auditEntityContent)\n      })\n    }\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n* {\n  box-sizing: border-box;\n}\nul,\nli {\n  list-style: none;\n}\n.audit {\n  width: 100%;\n  height: 100%;\n  background: #f0f3fa;\n}\n.audit_top {\n  width: 100%;\n  height: 70px;\n  background-color: rgba(255, 255, 255, 1);\n  box-shadow: 1.5px 2.6px 7px 0px rgba(0, 0, 0, 0.07);\n  border: 1px solid rgba(219, 224, 236, 1);\n  line-height: 70px;\n  padding-left: 30px;\n  cursor: pointer;\n}\n.audit_top span {\n  color: #333333;\n  font-size: 14px;\n  font-family: "微软雅黑";\n}\n\n.audit_box {\n  width: 100%;\n  height: calc(100% - 70px);\n}\n.audit_box_content {\n  width: 68%;\n  height: 94%;\n  background: red;\n  margin: 20px auto;\n  background-color: rgba(255, 255, 255, 1);\n  box-shadow: 1.5px 2.6px 7px 0px rgba(0, 0, 0, 0.07);\n  border: 1px solid rgba(219, 224, 236, 1);\n}\n\n.audit_user_time {\n  width: 100%;\n  height: 70px;\n  border-bottom: 1px solid #cccccc;\n  line-height: 70px;\n  padding-left: 30px;\n}\n.audit_user_time span {\n  colo