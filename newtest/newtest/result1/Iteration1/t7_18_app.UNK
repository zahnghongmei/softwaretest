loadPicBox\">\n      <div class=\"container\">\n        <div class=\"upload_top\">\n          <span style=\"float:left;\">上传图片</span>\n          <span class=\"iconfont icon-close\"\n                style=\"float:right;font-size:20px;cursor:pointer;\"\n                @click.stop='closeShowUploadBox()'></span>\n        </div>\n        <div class=\"upload_ele\">\n          <div class=\"upload_box upload_picture\"\n               v-for='item in imgList'>\n            <img :src=\"item\"\n                 alt=\"\" />\n          </div>\n          <div class=\"upload_box\"\n               v-show='isShowUpload'>\n            <el-upload ref=\"eluploadpic\"\n                       :action=\"actionUrl\"\n                       multiple\n                       :show-file-list=false\n                       list-type=\"picture-card\"\n                       :on-success=\"handleSuccess\"\n                       :headers='uploadHeaders'\n                       :on-progress='uploadProgress'\n                       :on-error=\"handleError\"\n                       :before-upload='beforeupload'\n                       :on-exceed=\"handleExceed\"\n                       :on-remove=\"handleRemove\"\n                       :limit='limitNum'>\n              <span class=\"add_pic_text\">上传图片</span>\n            </el-upload>\n          </div>\n        </div>\n\n        <div class=\"iseditor\"\n             style=\"cursor:pointer;\"\n             @click.stop='iseditorClick()'>\n          <div class=\"iseditor_icon\">\n            <img src=\"../../static/img/nochoice.png\"\n                 alt=\"\"\n                 v-if='!isEditorImg'>\n            <img src=\"../../static/img/clickchoice.png\"\n                 alt=\"\"\n                 v-if='isEditorImg'>\n          </div>\n          <span>是否编辑图片</span>\n        </div>\n\n        <div class=\"choice_save_file\">\n          <div class=\"choice_save_text\"\n               style=\"cursor:pointer;\"\n               @click.stop='choiceSaveFile()'>保存至</div>\n          <div class=\"choice_save_txt\"\n               style=\"margin-left:20px;border:none;\"\n               :title=\"repoFolderBreadName\">{{repoFolderBreadName}}</div>\n        </div>\n        <div class=\"share_button\">\n          <div class=\"sure_cancle\"\n               @click.stop='sureUploadPic()'>确&nbsp;&nbsp;定</div>\n          <div class=\"sure_cancle cancle_operate\"\n               @click.stop='closeShowUploadBox()'>取&nbsp;&nbsp;消</div>\n        </div>\n\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport * as api from '../../api'\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport Vue from 'vue'\nimport { insertEditorImg } from './registerButton.js'\nimport { loadingTranstion, closeLoading } from '../../helpers/loading.js'\nexport default {\n\n  components: {\n\n  },\n\n  created() {\n\n\n  },\n\n  mounted() {\n    this.$root.eventHub.$on('getUploadSaveFolder', (data, name) => {\n      this.uploadFolderInfo = data\n      this.choiceCurrentFolderName = name\n    })\n\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('getUploadSaveFolder')\n  },\n\n  data() {\n    return {\n      limitNum: 1,\n      imgList: [],\n      uploadFolderInfo: '',\n      isEditorImg: false,\n      isShowUpload: true,\n      choiceCurrentFolderName: ''\n    }\n  },\n\n  updated() {\n\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    showUploadPicBox() {\n      return this.$store.state.editor.showUploadPicBox\n    },\n    actionUrl() {\n      return api.baseURL + 'cache/upload_more/' + this.token + '/' + this.seed + '/' + this.userId + '/' + this.orgId\n    },\n    editorArticleObj() {\n      return this.$store.state.articlelist.editorArticleObj\n    },\n    repoFolderBreadName() {\n      return this.$store.state.repo.repoFolderBreadName\n    },\n    uploadHeaders() {\n      var option = {\n        token: this.token\n      }\n      return option\n    }\n\n\n\n\n  },\n\n  methods: {\n    handleExceed() {\n      toastr.warning('上传文件数不能超出限定范围！')\n    },\n    beforeupload(file) {\n\n    },\n    uploadProgress() {\n      loadingTranstion()\n    },\n    handleSuccess(response, file, fileList) {\n      closeLoading()\n      if (response) {\n        try {\n          let url = response.data[0]\n          if (url) {\n            this.imgList.push(url)\n          }\n          this.isShowUpload = false\n        } catch (e) {\n          console.error(e)\n        }\n      }\n\n    },\n\n    handleError(err, file, fileList) {\n      toastr.error('图片上传失败！')\n    },\n\n    handleRemove(file) {\n      if (!file) return\n      let url = ''\n      try {\n        url = file.response.data[0]\n      } catch (e) {\n        console.log('can\\'t find url!')\n      }\n      let list = this.imgList\n      let index = list.indexOf(url)\n      if (index !== -1) {\n        list.splice(index, 1)\n      }\n    },\n    choiceSaveFile() {\n      this.$root.eventHub.$emit('openFolder', '')\n    },\n    iseditorClick() {\n      this.isEditorImg = !this.isEditorImg\n    },\n    closeShowUploadBox() {\n      this.$store.commit('SET_SHOWUPLOAD_PICBOX', false)\n    },\n\n    sureUploadPic() {\n      if (this.imgList.length == 0) {\n        toastr.warning('请上传一张图片')\n      }\n      if (this.uploadFolderInfo == '') {\n        toastr.warning('请选择需要保存的文件夹')\n      }\n      var option = {\n        \"userId\": this.userId,\n        \"orgId\": this.orgId,\n        \"token\": this.token,\n        \"seed\": this.seed,\n        \"params\": {\n          \"picture_key\": this.imgList[0],  // 必须字段： 上传的图片的缓存 key.\n          \"id_repo\": this.uploadFolderInfo.id_repo || '',           // 可选字段： 库 id\n          \"id_folder\": this.uploadFolderInfo.id || '',       // 可选字段： 文件夹 id\n          \"title\": this.editorArticleObj.title || '',               // 可选字段： 图片名称（标题）\n          \"tags\": this.editorArticleObj.tags || [],               // 可选字段： 标签\n          \"keywords\": this.editorArticleObj.keywords || [],       // 可选字段： 关键词\n          \"detail\": this.editorArticleObj.detail || \"\"     // 可选字段： 描述\n        }\n      }\n      if (this.imgList.length > 0 && this.uploadFolderInfo != '') {\n        loadingTranstion()\n        api._post('material/picture/add_article_picture', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            closeLoading()\n            if (this.isEditorImg) {\n              this.$store.commit('SET_SHOWUPLOAD_PICBOX', false)\n              this.$store.commit('SET_EDITOR_PICDATAINFO', res.data.data)\n              this.getBase64Data(res.data.data)\n              this.$store.commit('SET_EDITOR_PICBOXSHOW', true)\n            } else {\n              insertEditorImg(res.data.data.url)\n              this.$store.commit('SET_SHOWUPLOAD_PICBOX', false)\n            }\n\n          } else {\n            closeLoading()\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n\n            }\n          }\n\n        }).catch((error) => {\n          console.error(error)\n          closeLoading()\n        })\n      }\n\n\n    },\n\n\n    //获取base64地址\n    getBase64Data(data) {\n      loadingTranstion()\n      api._post('material/picture/base64_picture?url=' + data.url + '&token=' + this.token, '').then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          this.$store.commit('SET_EDITORBASESIXFOUR_PICDATAINFO', resp.data.data)\n          closeLoading()\n        } else {\n          this.$store.commit('SET_EDITORBASESIXFOUR_PICDATAINFO', '')\n          closeLoading()\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n\n        