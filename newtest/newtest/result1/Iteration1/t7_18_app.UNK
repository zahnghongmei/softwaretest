se:hover {\n  background-color: #77A7FF;\n}","<template>\n  <transition name=\"fade\">\n    <div class=\"modal\"\n         v-show=\"show\">\n      <div class=\"container\"\n           @click.stop='showSearchUl=false'>\n        <div class=\"container_top\">\n          <span class=\"container_top_title\">送审至他人</span>\n          <span class=\"iconfont icon-close\"\n                v-on:click=\"closeModal()\"></span>\n        </div>\n        <div class=\"search_person\">\n          <input type=\"text\"\n                 placeholder='请输入姓名按回车键查询'\n                 @keyup.enter='searchPersonData()'\n                 v-model='searchPersonKey'\n                 @keyup.delete='cancleSearch()'\n                 @click.stop='getMoreUserData()'>\n          <img src=\"../../static/img/search.png\"\n               alt=\"\"\n               @click.stop='searchPersonData()'>\n          <div class=\"search_ul\"\n               v-show='showSearchUl'>\n            <ul class=\"search_ul_list\">\n              <li v-for='(item,index) in searchPersonList'\n                  @click.stop='addPerson(item)'>\n                <img :src=\"item.head_img\"\n                     alt=\"\"\n                     v-if='item.head_img'>\n                <img src=\"../../static/img/avatar.png\"\n                     alt=\"\"\n                     v-if='!item.head_img'>\n                <span>{{item.user_name}}</span>\n              </li>\n            </ul>\n\n          </div>\n        </div>\n        <div class=\"share_list\">\n          <div class=\"share_list_left\">\n            <div class=\"share_list_title\">组织或部门</div>\n            <ul class=\"share_list_content\">\n              <li v-for='(item,index) in depList'>\n                <div class=\"share_each\"\n                     @click.stop='getOrgChildPerson(item,index)'\n                     style=\"cursor:pointer;\">\n                  <span>\n                    <span class=\"iconfont icon-triangle-down\"\n                          v-if='isShowPerson[index]'></span>\n                    <span class=\"iconfont icon-triangle-right-big\"\n                          v-else></span>\n                  </span>\n                  <span class=\"sharch_name\">{{item.org_name}}</span>\n                  <div>\n                  </div>\n                </div>\n                <ul v-show='isShowPerson[index]'>\n                  <li v-for='(itemone,indexone) in item.person'\n                      @click.stop='choicePerson($event,itemone)'\n                      :class='\"person\"+itemone.id'\n                      style=\"cursor:pointer;\">\n                    <div class=\"share_each\">\n                      <img :src=\"itemone.head_img\"\n                           alt=\"\"\n                           v-if='itemone.head_img'>\n                      <img src=\"../../static/img/avatar.png\"\n                           alt=\"\"\n                           v-if='!itemone.head_img'>\n                      <span class=\"sharch_name sharch_name_second\">{{itemone.user_name}}</span>\n                      <div>\n                        <div class=\"share_choice\"></div>\n                        <div class=\"share_haschoice\">\n                          <span class=\"iconfont icon-check\"></span>\n                        </div>\n                      </div>\n                    </div>\n                  </li>\n                </ul>\n              </li>\n            </ul>\n          </div>\n          <div class=\"share_list_left share_list_right\">\n            <div class=\"share_list_title\">\n              <span>已选择人员</span>\n              <span style=\"color:#51BD90;\">{{userChoiceList.length}}</span>\n              <span>个</span>\n            </div>\n            <ul class=\"share_list_content\">\n              <li v-for='(item,index) in userChoiceList'>\n                <div class=\"share_each\">\n                  <span class=\"sharch_name\">{{item.user_name}}</span>\n                  <div>\n                    <div class=\"share_clear\"><span class=\"iconfont icon-close\"\n                            @click.stop='deleteChoicePerson(item,index)'></span></div>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n        <div class=\"share_button\">\n          <div class=\"sure_cancle\"\n               @click.stop='sureSubmitShare()'>确&nbsp;&nbsp;定</div>\n          <div class=\"sure_cancle cancle_operate\"\n               @click.stop='closeModal()'>取&nbsp;&nbsp;消</div>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport * as api from '../../api'\n// var debounce = require('throttle-debounce/debounce')\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport Vue from 'vue'\nexport default {\n\n  components: {\n\n  },\n\n  created() {\n\n\n  },\n\n  mounted() {\n    this.$root.eventHub.$on('setAuditArticleModal', (obj, type, key) => {\n      this.show = true\n      this.shareData = obj\n      this.personType = type\n      this.commentsText = key\n    })\n    niceScroller('.search_ul_list')\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setAuditArticleModal')\n  },\n\n  data() {\n    return {\n      show: false,\n      depList: [],\n      orgPersonList: [],\n      isShowPerson: [],\n      userChoiceList: [],\n      showSearchUl: false,\n      searchPersonKey: '',\n      searchPersonList: [],\n      shareData: '',\n      personType: '',\n      commentsText: ''\n    }\n  },\n\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller('.share_list_content')\n    this.scroller = niceScroller('.search_ul_list')\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    shareType() {\n      return this.$store.state.articlelist.shareType\n    }\n\n\n  },\n\n  methods: {\n    getDepData() {\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数，组织单位信息\n\n        }\n      }\n      api._post('org/get_org_info', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          this.depList = res.data.data || []\n          for (var i = 0; i < this.depList.length; i++) {\n            Vue.set(this.depList[i], \"person\", [])\n          }\n\n        } else {\n          this.depList = []\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        this.depList = []\n        console.error(error)\n      })\n    },\n    getOrgChildPerson(item, index) {\n      if (this.isShowPerson[index] == true) {\n        this.$set(this.isShowPerson, index, false)\n      } else {\n        this.$set(this.isShowPerson, index, true)\n        var option = {\n          \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n          \"token\": this.token,    // 必须字段：  token\n          \"seed\": this.seed,      // 必须字段：  seed\n          'orgId': this.orgId,\n          \"params\": {          // 必须字段：  参数，组织单位信息\n            \"id\": item.id\n          }\n        }\n        api._post('user/get_by_id_sub_org', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            this.orgPersonList = res.data.data || []\n            this.depList[index].person = this.orgPersonList\n            this.$nextTick(() => {\n              for (var i = 0; i < this.orgPersonList.length; i++) {\n                for (var j = 0; j < this.userChoiceList.length; j++) {\n                  if (this.orgPersonList[i].id == this.userChoiceList[j].id) {\n                    var id = '.' + 'person' + this.orgPersonList[i].id\n                    $(id).find('.share_choice').css('display', 'none')\n                    $(id).find('.share_haschoice').css('display', 'block')\n                  }\n                }\n              }\n\n            })\n\n          } else {\n            this.orgPersonList = []\n            this.depList[index].person = []\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          this.orgPersonList = []\n          this.depList[index].person = []\n          console.error(error)\n        })\n      }\n\n    },\n    choicePerson(event, item) {\n      // var id = event.currentTarget 'person' + item.id\n      var id = '.' + 'person' + item.id\n\n      if ($(id).find('.share_choice').css('display') == 'block' && $(id).find('.share_haschoice').css('display') == 'none') {\n        $('.share_choice').css('display', 'block')\n        $('.share_haschoice').css('display', 'none')\n        $(id).find('.share_choice').css('display', 'none')\n        $(id).find('.share_haschoice').css('display', 'block')\n        this.userChoiceList = [item]\n      } else {\n        $(id).find('.share_choice').css('display', 'block')\n        $(id).find('.share_haschoice').css('display', 'none')\n        this.userChoiceList = []\n      }\n\n    },\n    deleteChoicePerson(item, index) {\n      var id = '.' + 'person' + item.id\n      $(id).find('.share_choice').css('display', 'block')\n      $(id).find('.share_haschoice').css('display', 'none')\n      this.userChoiceList.splice(index, 1)\n    },\n    searchPersonData() {\n      if (this.searchPersonKey != '') {\n        this.showSearchUl = true\n        var option = {\n          \"userId\": this.userId,         // 必须字段：  用户 id，必须字段\n          \"token\": this.token,          // 必须字段：  token\n          \"seed\": this.seed,            // 必须字段：  seed\n          'orgId': this.orgId,\n          \"params\": {                // 必须字段：  参数，用户信息\n            \"keywords\": this.searchPersonKey      // 必须字段：  搜索的关键词\n          }\n        }\n        api._post('user/search_user', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            this.searchPersonList = res.data.data || []\n          } else {\n            this.searchPersonList = []\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          this.searchPersonList = []\n          console.error(error)\n        })\n      } else {\n        toastr.warning('请输入关键词')\n      }\n\n    },\n    cancleSearch() {\n      if (this.searchPersonKey.trim() == '') {\n        this.showSearchUl = false\n        this.searchPersonList = []\n      }\n    },\n    addPerson(item) {\n      this.userChoiceList = [item]\n      var id = '.' + 'person' + item.id\n      $('.share_choice').css('display', 'block')\n      $('.share_haschoice').css('display', 'none')\n      $(id).find('.share_choice').css('display', 'none')\n      $(id).find('.share_haschoice').css('display', 'block')\n    },\n\n    closeModal() {\n      this.show = false\n    },\n    sureSubmitShare() {\n      var arr = []\n      if (this.userChoiceList.length > 0) {\n        for (var i = 0; i < this.userChoiceList.length; i++) {\n          arr.push(this.userChoiceList[i].id)\n        }\n      }\n      var content_type = ''\n      var item_type = ''\n      if (this.personType == 'sendArticle') {\n        content_type = 'article'\n        item_type = 'article'\n      }\n      if (this.personType == 'video') {\n        content_type = 'video'\n        item_type = 'material'\n      }\n      if (this.personType == 'sendArticle' || this.personType == 'video') {\n        var option = {\n          \"userId\": this.userId,             // 必须字段：用户 id\n          \"orgId\": this.orgId,               // 必须字段：公司 id\n          \"token\": this.token,                // 必须字段： token\n          \"seed\": this.seed,                  // 必须字段： seed\n          \"params\": {                       // 必须字段： 参数\n            \"id\": this.shareData.id,              // 必须字段： 审核对象的 id\n            \"content_type\": content_type,  // 必须字段：类型  稿件为  article  视频为 video\n            \"id_to_user\": arr[0]        // 必须字段：指定的审核人的用户 ID\n          }\n        }\n        //获取审核日志\n        var auditLogOption = {\n          \"userId\": this.userId,             // 必须字段：用户 id\n          \"orgId\": this.orgId,               // 必须字段：公司 id\n          \"token\": this.token,                // 必须字段： token\n          \"seed\": this.seed,                  // 必须字段： seed\n          \"params\": {                       // 必须字段： 参数\n            \"id_content\": this.shareData.id,        // 必须字段：审核内容的 id（稿件 ID 或 视频 ID）\n            \"item_type\": item_type,    // 必须字段：类型  稿件为  article  视频为 material\n            \"page_no\": 0,                      // 可选字段：分页参数，页号，从 0 开始\n            \"page_size\": 1000                   // 可选字段：分页参数，每页长度，默认为 100\n          }\n        }\n        if (arr.length > 0) {\n          api._post('content_audit/audit_request', option).then((res) => {\n            if (res.data && res.data.code && res.data.code == 200) {\n              toastr.success('提交审核成功')\n              if (res.data.data) {\n                if (this.personType == \"sendArticle\") {\n                  this.$store.commit('SET_ARTICLE_AUDITSTATUS', res.data.data.audit_status)\n                  this.$store.commit('SET_ARTICLELIST_AUDITSTATUS', res.data.data)\n\n                }\n                if (this.personType == 'video') {\n                  this.$store.commit('SET_VIDEOAUDITSTATUS_CHANGE', res.data.data)\n                }\n\n              }\n              this.$store.dispatch('GET_AUDIT_LOGLIST', auditLogOption)\n              this.show = false\n            } else {\n              if (res.data && res.data.message) {\n                toastr.warning(res.data.message)\n              }\n            }\n\n          }).catch((error) => {\n            console.error(error)\n          })\n        } else {\n          toastr.error('请选择用户')\n        }\n\n      }\n      else if (this.personType == 'choiceNextAuditPerson') {\n        var option = {\n          \"userId\": this.userId,                // 必须字段：用户 id\n          \"orgId\": this.orgId,                  // 必须字段：公司 id\n          \"token\": this.token,                   // 必须字段： token\n          \"seed\": this.seed,                     // 必须字段： seed\n          \"params\": {                          // 必须字段： 参数\n            \"id\": this.shareData.id,               // 必须字段：审核条目的 ID\n            \"audit_status\": 'doing',  // 必须字段：审核通过为  yes  审核不通过为  no   审核通过并指定下一个审核人为  doing\n            \"id_to_user\": arr[0],  // 关联的可选（必须）字段：下一步审核人的 ID。当 audit_status 的值为 doing 时，此字段必须填。\n            \"comment\": this.commentsText            // 可选字段：审核的评论内容\n          }\n        }\n        if (arr.length > 0) {\n          api._post('content_audit/do_audit', option).then((res) => {\n            if (res.data && res.data.code && res.data.code == 200) {\n              toastr.success('提交成功')\n              this.$router.replace('/collectandsend/collecentry')\n              this.show = false\n            } else {\n              if (res.data && res.data.message) {\n                toastr.warning(res.data.message)\n              }\n            }\n\n          }).catch((error) => {\n            console.error(error)\n          })\n        } else {\n          toastr.error('请选择用户')\n        }\n\n      }\n\n    },\n    getMoreUserData() {\n      this.showSearchUl = true\n      var option = {\n        \"userId\": this.userId,         // 必须字段：  用户 id，必须字段\n        \"token\": this.token,          // 必须字段：  token\n        \"seed\": this.seed,            // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {                // 必须字段：  参数，用户信息\n\n        }\n      }\n      api._post('user/get_more_used_user', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          this.searchPersonList = res.data.data || []\n        } else {\n          this.searchPersonList = []\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        this.searchPersonList = []\n        console.error(error)\n      })\n    }\n\n  },\n\n  watch: {\n    show() {\n      if (this.show == false) {\n        this.userChoiceList = []\n        this.orgPersonList = []\n        this.isShowPerson = []\n        this.showSearchUl = false\n        this.searchPersonList = []\n        this.searchPersonKey = ''\n        $('.share_choice').css('display', 'block')\n        $('.share_haschoice').css('display', 'none')\n        this.shareData = ''\n      } else {\n        this.getDepData()\n      }\n    },\n    showSearchUl() {\n      if (this.showSearchUl == false) {\n        this.searchPersonList = []\n        this.searchPersonKey = ''\n      }\n    }\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n@import \"../../static/css/modal\";\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nul,\nli {\n  list-style: none;\n}\n.expand-transition {\n  transition: all 1s ease;\n}\n.expand-enter,\n.expand-leave {\n  opacity: 0;\n}\n.expand-enter .container,\n.expand-leave .container {\n  /*margin-top: -800px;*/\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n\n.container {\n  transition: all 1s ease;\n  width: 530px;\n  height: 560px;\n  position: absolute;\n  left: 40%;\n  top: 20%;\n  background-color: #ffffff;\n  box-shadow: 0px 0px 5px #999;\n  border-radius: 5px;\n  z-index: 999;\n  display: flex;\n  flex-direction: column;\n}\n\n.container_top {\n  width: 100%;\n  height: 44px;\n  background: #f7faff;\n  position: relative;\n  padding-left: 20px;\n  padding-right: 20px;\n  clear: both;\n  line-height: 44px;\n  border-bottom: 1px solid #c0beff;\n  margin-bottom: 10px;\n}\n.container_top .iconfont {\n  float: right;\n  font-size: 26px;\n  cursor: pointer;\n  color: #5f7094;\n}\n.container_top .container_top_title {\n  color: #333333;\n  font-size: 14px;\n  font-family: \"微软雅黑\";\n  float: left;\n}\n.search_person {\n  width: 490px;\n  height: 34px;\n  margin-bottom: 8px;\n  position: relative;\n  border: 1px solid #c8c8c8;\n  border-radius: 17px;\n  padding-left: 20px;\n  margin-left: 20px;\n  background: #fbfcff;\n}\n.search_person input {\n  width: 430px;\n  height: 32px;\n  border: none;\n  outline: none;\n  background: #fbfcff;\n  color: #666666;\n  font-size: 14px;\n}\n\n.search_person img {\n  position: absolute;\n  right: 20px;\n  top: 8px;\n  cursor: pointer;\n}\n\n.search_ul {\n  position: absolute;\n  border: 1px #c6cbd2 solid;\n  width: 200px;\n  height: 160px;\n  left: 0px;\n  top: 44px;\n  background-color: #fff;\n  z-index: 1099;\n  border-radius: 5px;\n  box-shadow: 0 0 3px 1px rgba(0, 0, 0, 0.2);\n}\n\n.search_ul:before {\n  content: \"\";\n  display: block;\n  position: absolute;\n  top: -17px;\n  left: 20px;\n  width: 0;\n  height: 0;\n  border-top: 8px solid transparent;\n  border-right: 8px solid transparent;\n  border-left: 8px solid transparent;\n  border-bottom: 8px solid #c6cbd2;\n}\n.search_ul:after {\n  content: \"\";\n  display: block;\n  position: absolute;\n  top: -17px;\n  left: 18px;\n  width: 0;\n  height: 0;\n  z-index: 2;\n  border-top: 10px solid transparent;\n  border-right: 10px solid transparent;\n  border-left: 10px solid transparent;\n  border-bottom: 10px solid #fff;\n}\n\n.search_ul ul {\n  margin: 0;\n  padding-top: 6px;\n  height: 100%;\n  overflow: hidden;\n}\n\n.search_ul li {\n  height: 34px;\n  line-height: 34px;\n  cursor: pointer;\n  padding-left: 32px;\n  position: relative;\n}\n\n.search_ul li:hover {\n  background-color: #eeeeee;\n}\n\n.search_ul li img {\n  vertica