s="editorimg_fn_container">\n          <div class="tailor"\n               @click.stop=\'changeTailoStatus()\'\n               :class="[showTailoDetail==false ? \'noborder\' : \'\']">\n            <span class="tailor_title iconfont icon-3caijian"\n                  style="font-size:16px"></span>\n            <span class="tailor_title tailor_text">裁剪</span>\n            <span class="tailor_right">\n              <span class="iconfont icon-triangle-right-big"\n                    style="font-size:14px;"\n                    v-show=\'!showTailoDetail\'></span>\n              <span class="iconfont icon-triangle-down"\n                    style="font-size:14px;"\n                    v-show=\'showTailoDetail\'></span>\n            </span>\n          </div>\n          <div class="tailo_children"\n               v-show=\'showTailoDetail\'>\n            <div class="width_height">\n              <div class="width_text">宽度：</div>\n              <div class="width_text"\n                   style="margin-left:30px;">高度：</div>\n            </div>\n            <div class="width_height">\n              <div class="width_text">\n                <input type="text"\n                       v-model=\'tailorPicWidth\'>\n              </div>\n              <div class="width_text"\n                   style="margin-left:30px;">\n                <input type="text"\n                       v-model=\'tailorPicHeight\'>\n              </div>\n            </div>\n\n            <div class="talior_scale">\n              <div class="positive_rotation"\n                   v-for=\'(item,index) in tailorScale\'\n                   :class="[tailorScaleIndex == index ? \'tailor_sacle_active\' : \'\']"\n                   @click.stop=\'tailorPicByScale(item,index)\'>{{item.x}}:{{item.y}}</div>\n              <div class="positive_rotation"\n                   @click.stop=\'freeCutting()\'\n                   :class="[tailorScaleIndex == 100 ? \'tailor_sacle_active\' : \'\']">自由裁剪</div>\n            </div>\n\n            <div class="lock_proportion"\n                 @click.stop=\'isLockTailorScale()\'\n                 style="cursor:pointer;">\n              <div style="padding-top:5px;cursor:pointer;">\n                <img src="../../static/img/clickchoice.png"\n                     alt=""\n                     v-show=\'lockTailorScale\'>\n                <img src="../../static/img/nochoice.png"\n                     alt=""\n                     v-show=\'!lockTailorScale\'>\n              </div>\n              <div class="lock_text">锁定裁剪宽高</div>\n            </div>\n\n            <div class="sure_cancle">\n              <div class="sure_tailor cancle_tailor"\n                   @click.stop=\'sureTailor()\'>确定</div>\n              <div class="cancle_tailor">取消</div>\n            </div>\n          </div>\n          <div class="tailor"\n               style="border-top:1px solid #c8c8c8;"\n               @click.stop=\'rotationPicClick()\'\n               :class="[rotationPic==false ? \'noborder\' : \'\']">\n            <span class="tailor_title iconfont icon-yulanxuanzhuan"></span>\n            <span class="tailor_title tailor_text">旋转</span>\n            <span class="tailor_right">\n              <span class="iconfont icon-triangle-right-big"\n                    style="font-size:14px"\n                    v-show=\'!rotationPic\'></span>\n              <span class="iconfont icon-triangle-down"\n                    style="font-size:14px"\n                    v-show=\'rotationPic\'></span>\n            </span>\n          </div>\n\n          <div class="tailo_children"\n               style="clear:both\'"\n               v-show=\'rotationPic\'>\n            <div class="positive_rotation"\n                 @click.stop=\'reverseRotate()\'>\n              反旋\n            </div>\n            <div class="positive_rotation"\n                 @click.stop=\'positiveRotate()\'>\n              正旋\n            </div>\n          </div>\n\n          <div class="tailor"\n               style="border-top:1px solid #c8c8c8;cursor:pointer;"\n               @click.stop=\'changeSizeStatus()\'\n               :class="[showSize==false ? \'noborder\' : \'\']">\n            <span class="tailor_title iconfont icon-chicun"\n                  style="font-size:16px;"></span>\n            <span class="tailor_title tailor_text">修改尺寸</span>\n            <span class="tailor_right">\n              <span class="iconfont icon-triangle-right-big"\n                    style="font-size:14px"\n                    v-show=\'!showSize\'></span>\n              <span class="iconfont icon-triangle-down"\n                    style="font-size:14px"\n                    v-show=\'showSize\'></span>\n            </span>\n          </div>\n          <div class="tailo_children"\n               style="padding-bottom:30px;"\n               v-show=\'showSize\'>\n\n            <div class="circle_route">尺寸缩放比例</div>\n            <div class="circle_range">\n              <input type="range"\n                     min="1"\n                     max="100"\n                     @change=\'sureChangePicSize()\'\n                     v-model=\'sizeLimit\'>\n            </div>\n\n          </div>\n\n          <div class="tailor"\n               style="border-top:1px solid #c8c8c8;cursor:pointer;"\n               :class="[adjustImg==false ? \'noborder\' : \'\']"\n               @click.stop=\'showAdjustImg()\'>\n            <span class="tailor_title iconfont icon-tiaozheng"\n                  style="font-size:22px;margin-left:-2px;"></span>\n            <span class="tailor_title tailor_text">调整</span>\n            <span class="tailor_right">\n              <span class="iconfont icon-triangle-right-big"\n                    style="font-size:14px"\n                    v-show=\'!adjustImg\'></span>\n              <span class="iconfont icon-triangle-down"\n                    style="font-size:14px"\n                    v-show=\'adjustImg\'></span>\n            </span>\n          </div>\n\n          <div class="tailo_children"\n               style="padding-top:0;"\n               v-show=\'adjustImg\'>\n            <div class="circle_route">亮度</div>\n            <div class="circle_range">\n              <input type="range"\n                     min="-100"\n                     max="100"\n                     @change=\'adjustPicBright()\'\n                     v-model=\'picBright\'>\n            </div>\n            <div class="circle_route">对比度</div>\n            <div class="circle_range">\n              <input type="range"\n                     min="-100"\n                     max="100"\n                     @change=\'adjustContrast()\'\n                     v-model=\'contrast\'>\n            </div>\n            <div class="circle_route">锐化</div>\n            <div class="circle_range">\n              <input type="range"\n                     min="50"\n                     max="399"\n                     @change=\'adjustSharpen()\'\n                     v-model=\'sharpen\'>\n            </div>\n            <div class="circle_route">清晰度</div>\n            <div class="circle_range">\n              <input type="range"\n                     min="1"\n                     max="50"\n                     v-model=\'fuzzyblur\'\n                     @change=\'adjustFuzzBlur()\'>\n            </div>\n          </div>\n          <div class="tailor"\n               style="border-top:1px solid #c8c8c8;cursor:pointer;border-bottom:0;"\n               @click.stop=\'addWaterMark()\'>\n            <span class="tailor_title iconfont icon-chicun"\n                  style="font-size:16px;"></span>\n            <span class="tailor_title tailor_text">添加水印</span>\n          </div>\n\n        </div>\n      </div>\n    </div>\n    <div class="editor_logs">\n      <div class="editor_logs_list">\n        <materiallog :show=true></materiallog>\n      </div>\n\n    </div>\n  </div>\n\n</template>\n  \n<script>\nimport vueCropper from \'vue-cropper\'\nimport { niceScroller } from \'../../helpers/niceScroller\'\nimport * as api from \'../../api\'\nimport materiallog from \'./materiallog.vue\'\nimport { loadingTranstion, closeLoading } from \'../../helpers/loading.js\'\nexport default {\n  name: \'editorimg\',\n  components: {\n    vueCropper,\n    materiallog\n  },\n\n  data() {\n    return {\n      showTailoDetail: false,\n      showSize: false,\n      editorPicData: \'\',\n      adjustImg: false,\n      tailorScale: [{ x: 1, y: 1 }, { x: 3, y: 4 }, { x: 4, y: 3 }, { x: 16, y: 9 }, { x: 9, y: 16 }],\n      lockTailorScale: true,\n      tailorPicWidth: 300,\n      tailorPicHeight: 300,\n      tailorScaleIndex: 99999999,\n      editorImgRealWidth: 10,\n      editorImgRealHeight: 10,\n      curretnPicWidth: 800,\n      currentPicHeight: 800,\n      rotationPic: false,\n      rotateDeg: 0,\n      picBright: 0,\n      contrast: 0,\n      sharpen: 50,\n      fuzzyblur: 50,\n      sizeLimit: 100,\n      currentImgUrl: \'\',\n      editorimgOption: {\n        img: \'\',\n        size: 1,\n        full: false,\n        outputType: \'png\',\n        canMove: true,\n        fixedBox: true,\n        original: true,\n        canMoveBox: true,\n        autoCrop: false,\n        info: true,\n        canScale: true,\n        centerBox: true,\n        autoCropWidth: 300,\n        autoCropHeight: 300,\n        fixed: false,\n        fixedNumber: [1, 1]\n\n      },\n      downImg: \'#\'\n    }\n  },\n  destroyed() {\n    this.$root.eventHub.$off(\'editorImgData\')\n    this.$root.eventHub.$off(\'waterMarkPic\')\n    this.$root.eventHub.$off(\'clearEditorPicData\')\n  },\n  beforeDestroy() {\n\n  },\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller(\'.editor_logs_list\')\n  },\n  mounted() {\n    this.$root.eventHub.$on(\'editorImgData\', (data) => {\n      this.$refs.cropper.refresh()\n      this.$refs.cropper.clearCrop()\n      this.editorPicData = data\n    })\n\n    this.$root.eventHub.$on(\'clearEditorPicData\', (data) => {\n      this.clearTailorData()\n      this.clearStatus()\n    })\n\n\n\n\n  },\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    waterMarkUrl() {\n      return this.$store.state.editor.waterMarkUrl\n    },\n    editorPicInfoData() {\n      return this.$store.state.page.editorPicInfoData\n    },\n    base64ImgInfo() {\n      return this.$store.state.page.base64ImgInfo\n    },\n    isShowInnerImg() {\n      return this.$store.state.page.isShowInnerImg\n    }\n  },\n  methods: {\n    //清除数据\n    clearTailorData() {\n      this.editorimgOption = {\n        img: \'\',\n        size: 1,\n        full: false,\n        outputType: \'png\',\n        canMove: true,\n        fixedBox: true,\n        original: false,\n        canMoveBox: true,\n        autoCrop: false,\n        info: true,\n        canScale: true,\n        centerBox: true,\n        autoCropWidth: 300,\n        autoCropHeight: 300,\n        fixed: false,\n        fixedNumber: [1, 1]\n      }\n      this.lockTailorScale = true\n      this.tailorScaleIndex = 999999999\n      // this.showTailoDetail = false\n\n    },\n    clearStatus() {\n      this.rotateDeg = 0\n      this.picBright = 0\n      this.contrast = 0\n      this.sharpen = 50\n      this.fuzzyblur = 50\n      this.sizeLimit = 100\n      this.showSize = false\n      this.adjustImg = false\n      this.rotationPic = false\n      this.showTailoDetail = false\n      this.tailorScaleIndex = 999999999\n      this.lockTailorScale = true\n\n    },\n    // 获取图片真实宽高\n    getImgRealWidthHeight(url) {\n      api._post(\'material/picture/oss_picture_info?url=\' + url + \'&token=\' + this.token, \'\').then((resp) => {\n        if (resp.data.code == 200) {\n          this.editorImgRealWidth = resp.data.data.width\n          this.editorImgRealHeight = resp.data.data.height\n\n        } else {\n          toastr.warning(resp.data.message)\n        }\n      }).catch((error) => {\n        toastr.error(error)\n      })\n\n\n    },\n    changeTailoStatus() {\n      this.$refs.cropper.refresh()\n      this.showTailoDetail = !this.showTailoDetail\n      if (this.showTailoDetail == true) {\n        this.$store.commit(\'SET_ISSHOWINNER_IMG\', false)\n        this.editorimgOption.autoCrop = true\n        this.tailorPicWidth = 300\n        this.tailorPicHeight = 300\n      } else {\n        this.editorimgOption.autoCrop = false\n        this.$store.commit(\'SET_ISSHOWINNER_IMG\', true)\n      }\n    },\n    changeSizeStatus() {\n      this.showSize = !this.showSize\n      if (this.showSize == true) {\n        this.editorimgOption.autoCrop = false\n      }\n\n    },\n    showAdjustImg() {\n      this.adjustImg = !this.adjustImg\n      if (this.adjustImg == true) {\n        this.editorimgOption.autoCrop = false\n      }\n\n    },\n    isLockTailorScale() {\n      this.lockTailorScale = !this.lockTailorScale\n      this.$refs.cropper.refresh()\n      this.editorimgOption.autoCrop = true\n      if (this.lockTailorScale == true) {\n        this.editorimgOption.fixedBox = true\n        this.tailorPicWidth = 300\n        this.tailorPicHeight = 300\n        this.tailorScaleIndex = 99999999999\n      } else {\n        this.editorimgOption.fixedBox = false\n      }\n      this.editorimgOption.fixed = false\n\n    },\n    tailorPicByScale(item, index) {\n      this.tailorScaleIndex = index\n      this.lockTailorScale = false\n      this.editorimgOption.autoCrop = true\n      this.editorimgOption.fixed = true\n      this.editorimgOption.fixedBox = false\n      this.editorimgOption.autoCropWidth = 100\n      this.editorimgOption.autoCropHeight = 100\n      this.editorimgOption.fixedNumber = [item.x, item.y]\n    },\n    freeCutting() {\n      this.editorimgOption.autoCrop = true\n      this.lockTailorScale = false\n      this.tailorScaleIndex = 100\n      this.editorimgOption.fixedBox = false\n      this.editorimgOption.fixed = false\n      this.editorimgOption.autoCropWidth = 300\n      this.editorimgOption.autoCropHeight = 300\n    },\n    sureTailor() {\n      this.$refs.cropper.getCropData((data) => {\n        var option = {\n          "userId": this.userId,        // 必须字段：用户 ID\n          "orgId": this.orgId,          // 必须字段：企业 ID\n          "token": this.token,          // 必须字段：token\n          "seed": this.seed,            // 必须字段：seed\n          "params": {                 // 必须字段：编辑参数\n            "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n            "optname": "crop_base64",        // 必须字段：图片裁剪，固定值：crop\n            "params": {\n              "factor": 1,        // 必须字段：原图缩放参数\n              "x": \'\',              // 必须字段：裁剪起点的 x 坐标\n              "y": \'\',              // 必须字段：裁剪起点的 y 坐标\n              "w": \'\',              // 必须字段：裁剪宽度\n              "h": \'\',       // 必须字段：裁剪高度\n              \'base64\': data\n            }\n          }\n        }\n        this.getEditorPicUrl(option)\n\n      })\n\n\n    },\n    rotationPicClick() {\n      this.rotationPic = !this.rotationPic\n      if (this.rotationPic == true) {\n        this.editorimgOption.autoCrop = false\n      }\n\n    },\n    positiveRotate() {\n      // this.rotateDeg += 90\n      // if (this.rotateDeg == 360) {\n      //   this.rotateDeg = 0\n      // }\n      this.rotateDeg = 90\n      this.getRotatePicUrl(this.rotateDeg)\n    },\n\n    reverseRotate() {\n      // if (this.rotateDeg == 0) {\n      //   this.rotateDeg = 270\n      // } else {\n      //   this.rotateDeg -= 90\n      // }\n      this.rotateDeg = 270\n      this.getRotatePicUrl(this.rotateDeg)\n    },\n    getRotatePicUrl(deg) {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "rotate",        // 必须字段：图片旋转，固定值：rotate\n          "params": {\n            "value": deg        // 必须字段：旋转角度，取值范围： 0 到 360\n          }\n        }\n      }\n\n      this.getEditorPicUrl(option)\n    },\n    //改变亮度\n    adjustPicBright() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "bright",\n          "params": {\n            "value": Number(this.picBright)\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n    },\n    //改变对比度\n    adjustContrast() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "contrast",\n          "params": {\n            "value": Number(this.contrast)\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n\n    },\n\n    //图片锐化\n    adjustSharpen() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "sharpen",\n          "params": {\n            "value": Number(this.sharpen)\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n    },\n    //模糊程度\n    adjustFuzzBlur() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "blur",\n          "params": {\n            "r": Number(this.fuzzyblur),\n            \'s\': Number(this.fuzzyblur)\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n    },\n    //修改尺寸\n    sureChangePicSize() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "resize",\n          "params": {\n            \'limit\': 1,\n            \'p\': Number(this.sizeLimit)\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n\n    },\n\n    clearPic() {\n      this.clearStatus()\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "clean",\n          "params": {\n\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n\n    },\n    savePic() {\n      this.clearStatus()\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段：编辑参数\n          "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n          "optname": "save",\n          "params": {\n\n          }\n        }\n      }\n      this.getEditorPicUrl(option)\n    },\n    addWaterMark() {\n      this.$store.commit(\'SET_SHOWWATERMARK_STATUS\', true)\n    },\n\n\n    getEditorPicUrl(option) {\n      loadingTranstion()\n      api._post(\'material/picture/picture_editor\' + \'?type=\' + \'material\', option).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          closeLoading()\n          this.clearTailorData()\n          this.showTailoDetail = false\n          this.$store.commit(\'SET_ISSHOWINNER_IMG\', true)\n          //this.editorimgOption.img = resp.data.data.url\n          // this.editorimgOption.img = api.baseURL + \'material/picture/raw_picture?url=\' + resp.data.data.url + \'&token=\' + this.token\n          this.editorimgOption.img = \'data:image;base64,\' + resp.data.data.img_base64\n          this.currentImgUrl = resp.data.data.url\n          this.$store.commit(\'SET_MODIFT_PIC\', resp.data.data)\n          var logoption = {\n            "userId": this.userId,             // 必须字段：用户 id\n            "orgId": this.orgId,               // 必须字段：企业 id\n            "token": this.token,                // 必须字段： token\n            "seed": this.seed,                  // 必须字段： seed\n            "params": {                       // 必须字段： 参数\n              "id": this.editorPicData.id,           // 必须字段： 图片 ID\n            }\n          }\n          this.$store.dispatch(\'GET_PIC_LOGLIST\', logoption)\n\n        } else {\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n          closeLoading()\n        }\n      }).catch((error) => {\n        toastr.error(error)\n        closeLoading()\n      })\n    }\n\n  },\n  watch: {\n    waterMarkUrl() {\n      if (this.waterMarkUrl != \'\') {\n        var option = {\n          "userId": this.userId,        // 必须字段：用户 ID\n          "orgId": this.orgId,          // 必须字段：企业 ID\n          "token": this.token,          // 必须字段：token\n          "seed": this.seed,            // 必须字段：seed\n          "params": {                 // 必须字段：编辑参数\n            "id_picture": this.editorPicData.id,  // 必须字段：图片素材 id\n            "optname": "watermark",\n            "params": {\n              "water_mark_url": this.waterMarkUrl,  // 必须字段：水印图片的 url\n            }\n          }\n        }\n        this.getEditorPicUrl(option)\n      }\n\n    },\n    editorPicInfoData() {\n      if (this.editorPicInfoData != \'\') {\n        this.$nextTick(() => {\n          this.currentImgUrl = this.editorPicInfoData.url\n        })\n\n      }\n    },\n    base64ImgInfo() {\n      if (this.base64ImgInfo != \'\') {\n        this.$nextTick(() => {\n          this.editorimgOption.img = \'data:image;base64,\' + this.base64ImgInfo\n        })\n\n      }\n    },\n    tailorPicWidth() {\n      this.lockTailorScale = true\n      this.$refs.cropper.refresh()\n      this.editorimgOption.autoCrop = true\n      this.editorimgOption.fixedBox = true\n      this.editorimgOption.fixed = false\n      this.tailorScaleIndex = 99999999999\n      this.editorimgOption.autoCropWidth = Number(this.tailorPicWidth)\n      this.editorimgOption.autoCropHeight = Number(this.tailorPicHeight)\n\n    },\n    tailorPicHeight() {\n      this.lockTailorScale = true\n      this.$refs.cropper.refresh()\n      this.editorimgOption.autoCrop = true\n      this.editorimgOption.fixedBox = true\n      this.editorimgOption.fixed = false\n      this.tailorScaleIndex = 99999999999\n      this.editorimgOption.autoCropHeight = Number(this.tailorPicHeight)\n      this.editorimgOption.autoCropWidth = Number(this.tailorPicWidth)\n\n    }\n  }\n}\n\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to th