     this.templateObj = ''\n        this.columnTemplate = []\n      }\n\n\n    })\n\n    this.$root.eventHub.$on('setColumnChildren', (data) => {\n      this.handleNodeClick(data)\n    })\n\n    this.$root.eventHub.$on('deleteColumn', (obj) => {\n      for (var i = 0; i < this.sendPushData.length; i++) {\n        if (this.sendPushData[i].id_ch == obj.id_ch) {\n          this.sendPushData.splice(i, 1);\n          break;\n        }\n      }\n\n    })\n\n\n\n\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setPushModel')\n    this.$root.eventHub.$off('setColumnTemplate')\n    this.$root.eventHub.$off('setColumnChildren')\n    this.$root.eventHub.$off('deleteColumn')\n  },\n\n  data() {\n    return {\n      show: false,\n      portType: 'pc',\n      pushObj: '',\n      columnTemplate: [],\n      dataList: [],\n      templateObj: '',\n      sendPushData: [],\n      hasChoiceTemplateList: [],\n      channelKeyWords: '',\n      templateKeyWords: ''\n\n    }\n  },\n\n  updated() {\n\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n\n  },\n\n  methods: {\n    closeModal() {\n      this.show = false\n    },\n    getEveryPort(type, event) {\n      this.portType = type\n      $('.container_send_each').removeClass('container_send_eachActive')\n      $(event.currentTarget).addClass('container_send_eachActive')\n      if (type == 'app') {\n        this.$root.eventHub.$emit('setPushInfo', this.pushObj)\n        //this.$root.eventHub.$emit('setPushZhongWangApp', this.pushObj)\n\n      }\n      if (type == 'headlines') {\n        this.$root.eventHub.$emit('setPushGuanshanappInfo', this.pushObj)\n      }\n      // if (type == 'pc') {\n      //   this.$root.eventHub.$emit('setPushPcInfo', this.pushObj)\n      // }\n      // if (type == 'qiehao') {\n      //   this.$root.eventHub.$emit('setPushSiNanAppInfo', this.pushObj)\n      // }\n    },\n    handleNodeClick(data) {\n      var option = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.seed,                  // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": {                       // 必须字段： 请求参数\n          \"id_ch\": data.id_ch    // 必须字段：频道ID\n        }\n      }\n      loadingTranstion()\n      api._post('pusher/gog_north_net/channel/get_children', option).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          closeLoading()\n          Vue.set(data, 'children', resp.data.data)\n\n        } else {\n          closeLoading()\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n        }\n      }).catch((error) => {\n        closeLoading()\n        toastr.error(error)\n      })\n\n    },\n\n    getColumnRootData() {\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数\n\n        }\n      }\n      loadingTranstion()\n      api._post('pusher/gog_north_net/channel/get_root', option).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          closeLoading()\n          this.dataList = resp.data.data\n          this.dataList.forEach((item) => {\n            Vue.set(item, 'children', [])\n\n          })\n\n        } else {\n          closeLoading()\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n        }\n      }).catch((error) => {\n        closeLoading()\n        toastr.error(error)\n      })\n    },\n\n    //搜索栏目\n    searColumnRootData() 