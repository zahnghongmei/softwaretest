ewstopicarticle from \'../wrapperright/newstopicarticle.vue\'\nimport insertvideolist from \'./insertvideolist.vue\'\nimport insertvideoissue from \'./insertvideoissue.vue\'\nimport insertaudiolist from \'./insertaudiolist.vue\'\nimport saveas from \'../member/saveas.vue\'\nimport auditsure from \'../member/auditsure.vue\'\nimport insertimgtoeditor from \'./insertimgtoeditor.vue\'\nimport editorimglist from \'./editorimglist.vue\'\nexport default {\n  name: \'editormain\',\n  components: {\n    uploadarticlepic,\n    savefolder,\n    choiceuploadtype,\n    editorimgbox,\n    watermark,\n    marerialchoicepic,\n    auditperson,\n    videoplay,\n    pictureview,\n    audioplayer,\n    newstopicarticle,\n    insertvideolist,\n    insertvideoissue,\n    insertaudiolist,\n    saveas,\n    auditsure,\n    insertimgtoeditor,\n    editorimglist\n  },\n  data() {\n    return {\n    }\n  },\n  created() {\n    this.getMaterialData()\n    if (this.editorArticleObj == \'\') {\n      var localObj = localStorage.getItem("editorManuscriptInfo")\n      var obj = \'\'\n      if (localObj) {\n        obj = JSON.parse(localObj)\n        this.getAllArticleDataInfo(obj)\n        //this.getMaterialData()\n        //this.getWaterMark()\n      }\n      this.$store.commit(\'SET_EDITORDATA_ITEM\', obj)\n      this.$store.commit(\'SET_COMMENTS_ID\', obj.id)\n    }\n\n    if (this.editorRepoId == \'\' && this.editorFoldId == \'\') {\n      var option = {\n        editorRepoId: localStorage.getItem("editorRepoId"),\n        editorFoldId: localStorage.getItem("editorFoldId")\n      }\n      this.$store.commit(\'SET_REPOANDFOLD_ID\', option)\n      this.$store.commit(\'SET_REPOSITORY_ID\', localStorage.getItem("editorFoldId"))\n    }\n    if (this.repoObj == \'\' && this.folderFirstData == \'\' && this.folderSecondData == \'\') {\n      var localRepoObj = localStorage.getItem("repoObj")\n      if (localRepoObj) {\n        this.$store.commit(\'SET_REPO_OBJDATA\', JSON.parse(localRepoObj))\n      }\n      var localfolderFirstData = localStorage.getItem("folderFirstData")\n      if (localfolderFirstData) {\n        this.$store.commit(\'SET_FOLDERFIRST_DATA\', JSON.parse(localfolderFirstData))\n      }\n      var localfolderSecondData = localStorage.getItem("folderSecondData")\n      if (localfolderSecondData) {\n        this.$store.commit(\'SET_FOLDERSECOND_DATA\', JSON.parse(localfolderSecondData))\n      }\n\n      var localfolderLevelData = localStorage.getItem("folderLevelData")\n      if (localfolderLevelData) {\n        this.$store.commit(\'SET_FOLDERLEVEL_DATA\', JSON.parse(localfolderLevelData))\n      }\n\n    }\n\n    window.addEventListener("beforeunload", () => {\n      if (this.$store.state.articlelist.editorArticleObj) {\n        localStorage.setItem("editorManuscriptInfo", JSON.stringify(this.$store.state.articlelist.editorArticleObj))\n      }\n      if (this.$store.state.articlelist.editorRepoId) {\n        localStorage.setItem("editorRepoId", this.$store.state.articlelist.editorRepoId)\n      }\n      if (this.$store.state.articlelist.editorFoldId) {\n        localStorage.setItem("editorFoldId", this.$store.state.articlelist.editorFoldId)\n      }\n      if (this.$store.state.repo.repoObj) {\n        localStorage.setItem("repoObj", JSON.stringify(this.$store.state.repo.repoObj))\n      }\n      if (this.$store.state.repo.folderFirstData) {\n        localStorage.setItem("folderFirstData", JSON.stringify(this.$store.state.repo.folderFirstData))\n      }\n      if (this.$store.state.repo.folderSecondData) {\n        localStorage.setItem("folderSecondData", JSON.stringify(this.$store.state.repo.folderSecondData))\n      }\n      if (this.$store.state.repo.folderLevelData) {\n        localStorage.setItem("folderLevelData", JSON.stringify(this.$store.state.repo.folderLevelData))\n      }\n\n    })\n  },\n  beforeDestroy() {\n\n  },\n  mounted() {\n\n  },\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    editorArticleObj() {\n      return this.$store.state.articlelist.editorArticleObj\n    },\n    editorRepoId() {\n      return this.$store.state.articlelist.editorRepoId\n    },\n    editorFoldId() {\n      return this.$store.state.articlelist.editorFoldId\n    },\n    repoObj() {\n      return this.$store.state.repo.repoObj\n    },\n    folderFirstData() {\n      return this.$store.state.repo.folderFirstData\n    },\n    folderSecondData() {\n      return this.$store.state.repo.folderSecondData\n    },\n    folderLevelData() {\n      return this.$store.state.repo.folderLevelData\n    }\n\n  },\n  methods: {\n    closeEditorBox() {\n      this.$root.eventHub.$emit(\'closePersonBox\', \'\')\n      this.$root.eventHub.$emit(\'closeWrongName\', \'\')\n    },\n    //获取素材库\n    getMaterialData() {\n      var option = {\n        "userId": this.userId,  // 必须字段：  用户 id，必须字段\n        "token": this.token,    // 必须字段：  token\n        "seed": this.seed,      // 必须字段：  seed\n        \'orgId\': this.orgId,\n        "params": {          // 必须字段：  参数\n          "flag_type": \'material\',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch(\'GET_MATERIAL_DATA\', option)\n    },\n\n    //获取水印列表\n    getWaterMark() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {                 // 必须字段： 参数\n        }\n      }\n      this.$store.dispatch(\'GET_WATERMARKLIST_DATA\', option)\n\n    },\n    getAllArticleDataInfo(item) {\n      //获取评论\n      var comments = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n          "page_no": 0,                  // 可选字段： 分页，页号，从 0 开始， 默认 0\n          "page_size": 1000               // 可选字段： 分页，返回数量，默认 10，最大 100\n        }\n      }\n      this.$store.dispatch(\'GET_ARTICLE_COMMENTS\', comments)\n      //获取日志\n      var logoption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n        }\n      }\n\n      this.$store.dispatch(\'GET_LOG_OPERATEDATA\', logoption)\n      //获取历史记录也就是留痕\n      var historyOption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n          "page_no": 0,                  // 可选字段： 分页，页号，从 0 开始， 默认 0\n          "page_size": 1000               // 可选字段： 分页，返回数量，默认 10，最大 100\n        }\n      }\n      this.$store.dispatch(\'GET_HISTORY_RECORD\', historyOption)\n\n      //获取审核日志\n      var auditLogOption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "orgId": this.orgId,               // 必须字段：公司 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        "params": {                       // 必须字段： 参数\n          "id_content": item.id,        // 必须字段：审核内容的 id（稿件 ID 或 视频 ID）\n          "item_type": "article",    // 必须字段：类型  稿件为  article  视频为 material\n          "page_no": 0,                      // 可选字段：分页参数，页号，从 0 开始\n          "page_size": 1000                   // 可选字段：分页参数，每页长度，默认为 100\n        }\n      }\n      this.$store.dispatch(\'GET_AUDIT_LOGLIST\', auditLogOption)\n\n      //获取报题信息\n      if (item.flag_sys_source == "news_recover") {\n        this.$store.commit(\'SET_NEWSTOPICINFO_DATA\', \'\')\n        var newstopicOption = {\n          "userId": this.userId,              // 必须字段：用户 id\n          "orgId": this.orgId,                // 必须字段：org id\n          "token": this.token,                 // 必须字段： token\n          "seed": this.seed,                   // 必须字段： seed\n          "params": {                        // 必须字段： 创建收藏记录参数\n            "type": "article",     // 必须字段： 类型，文章：article  素材： material\n            "id": item.id,               // 必须字段： 文章 ID 或者 是素材 ID\n          }\n        }\n        this.$store.dispatch(\'GET_NEWSTOPIC_INFODATA\', newstopicOption)\n\n      }\n\n\n    },\n\n  },\n\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only --\x3e\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\n.editor_page {\n  width: 100%;\n  height: 100%;\n  min-width: 1366px;\n}\n</style>\n'],sourceRoot:""}])},Zonf:function(n,t){n.exports="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABMAAAAOCAYAAADNGCeJAAAAAXNSR0IArs4c6QAAADRJREFUOBFjTE9P3/r//38vBgoBIyPjNiYKzYBrBzroP5wzyiA6BBhHY5PosBr8CgdvbAIAjAUdvND+ByMAAAAASUVORK5CYII="},ZwAp:function(n,t,e){(n.exports=e("FZ+f")(!0)).push([n.i,'\n.modal[data-v-cb99af94] {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  z-index: 1000;\n}\n.fade-enter-active[data-v-cb99af94] {\n  -webkit-animation: fade-in-data-v-cb99af94 .3s;\n          animation: fade-in-data-v-cb99af94 .3s;\n}\n.fade-leave-active[data-v-cb99af94] {\n  -webkit-animation: fade-out-data-v-cb99af94 .3s;\n          animation: fade-out-data-v-cb99af94 .3s;\n}\n@-webkit-key