.commit(\'SET_PAGE_NOW\', 1)\n      this.$store.commit(\'SET_CLEARPICLIST_DATA\')\n      this.$store.commit(\'SET_SINGLEOR_PROJECTTYPE\', type)\n      this.$root.eventHub.$emit(\'clearKeyWords\', \'\')\n      if (this.picType == \'single\') {\n        this.$store.commit(\'SET_ORGRIANLARTICLE_CONDITIONOPTION\')\n        var materialObjOption = JSON.parse(JSON.stringify(this.materialConditionData))\n        materialObjOption.id_folder = this.editorFoldId\n        materialObjOption.page_no = 0\n        materialObjOption.page_size = this.pageSize\n\n        var singleoption = {\n          "userId": this.userId,        // 必须字段：用户 ID\n          "orgId": this.orgId,          // 必须字段：企业 ID\n          "token": this.token,          // 必须字段：token\n          "seed": this.seed,            // 必须字段：seed\n          "params": materialObjOption\n        }\n        if (this.editorFoldId) {\n          this.$store.dispatch(\'GET_MATERIAL_SINGLEPICLIST\', singleoption)\n        } else {\n          toastr.info(\'请选择文件夹哦\')\n        }\n\n      } else if (this.picType == \'special\') {\n        var projectoption = {\n          "userId": this.userId,        // 必须字段：用户 ID\n          "orgId": this.orgId,          // 必须字段：企业 ID\n          "token": this.token,          // 必须字段：token\n          "seed": this.seed,            // 必须字段：seed\n          "params": {\n            "id_folder": this.editorFoldId,   // 必须字段： 文件夹 ID\n            "page_no": 0,               // 可选字段：分页的页号，从 0 开始\n            "page_size": this.pageSize,             // 可选字段：每页条数，从 1 开始，默认 10\n            \'keywords\': \'\'\n          }\n        }\n        if (this.editorFoldId) {\n          this.$store.dispatch(\'GET_MATERIAL_PROJECTPICLIST\', projectoption)\n        } else {\n          toastr.info(\'请选择文件夹哦\')\n        }\n\n      }\n\n    },\n\n    lookPicInfo(item) {\n      this.$store.commit(\'SET_PAGE_NOW\', 1)\n      if (this.picType == \'single\') {\n        if (this.isShowOperate) {\n          this.$root.eventHub.$emit(\'