ta.keywords\n              var arr = []\n              for (var key in currentReapData) {\n                arr.push(key)\n              }\n              this.addManuscriptData.keywords = arr\n            }\n\n          } else {\n            this.loading && this.loading.close()\n            if (resp.data && resp.data.message) {\n              toastr.warning(resp.data.message)\n            }\n          }\n        }).catch((error) => {\n          this.loading && this.loading.close()\n          toastr.error(error)\n        })\n      }\n\n    },\n    handleSummary(editor) {\n      var editor = this.ueditorIns\n      var content = editor.getContentTxt()\n      var option = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 参数\n          "content": content,     // 必须字段： 文章内容\n          "id": "",               // 可选字段: 文章ID\n          "title": ""         // 可选字段： 文章标题\n        }\n      }\n      if (this.addManuscriptData.summary) {\n        toastr.info(\'已经有摘要了\')\n      } else {\n        this.loading = this.$loading({\n          fullscreen: false,\n          text: \'正在获取摘要，请稍后...\'\n        })\n        api._post(\'third_party/ai_checker/get_summary\', option).then((resp) => {\n          if (resp.data && resp.data.code && resp.data.code == 200) {\n            this.loading && this.loading.close()\n            if (resp.data.data.summary) {\n              this.addManuscriptData.summary = resp.data.data.summary\n            }\n\n          } else {\n            this.loading && this.loading.close()\n            if (resp.data && resp.data.message) {\n              toastr.warning(resp.data.message)\n            }\n          }\n        }).catch((error) => {\n          this.loading && this.loading.close()\n          toastr.error(error)\n        })\n      }\n\n\n    },\n    //分析检测\n    handleAnalysis(editor