Content:["<template>\n  <div class=\"materialcontent\">\n    <div class=\"material_left\">\n      <wrapperleft :isShowDownload=true></wrapperleft>\n    </div>\n    <div class=\"material_right\">\n      <div class=\"all_media_top\"\n           v-if='isShowTop'>\n        <userinfotop :showReturn='isReturn'\n                     :isShowNav='isShowTopNav'></userinfotop>\n      </div>\n      <div class=\"material_single_pic\"\n           v-show='currentPicType==\"single\"'>\n        <div class=\"material_nav\">\n          <div class=\"material_nav_content\">\n            <div class=\"material_title\">\n              <div class=\"material_nav_each\"\n                   :class=\"[materialType=='pic' ? 'material_nav_eachactive' : '']\"\n                   @click.stop='choiceMaterialType(\"pic\")'>图片</div>\n              <div class=\"material_nav_each\"\n                   :class=\"[materialType=='video' ? 'material_nav_eachactive' : '']\"\n                   @click.stop='choiceMaterialType(\"video\")'>视频</div>\n              <div class=\"material_nav_each\"\n                   :class=\"[materialType=='audio' ? 'material_nav_eachactive' : '']\"\n                   @click.stop='choiceMaterialType(\"audio\")'>音频</div>\n              \x3c!--  <div class=\"material_nav_each\" :class=\"[materialType=='text' ? 'material_nav_eachactive' : '']\" @click.stop='choiceMaterialType(\"text\")'>文字</div> --\x3e\n              <div class=\"material_nav_each\"\n                   :class=\"[materialType=='attachment' ? 'material_nav_eachactive' : '']\"\n                   @click.stop='choiceMaterialType(\"attachment\")'>附件</div>\n            </div>\n\n            <div class=\"material_filter\">\n              <div class=\"all_key_down\"\n                   @click.stop='showSearcyTypeCondition()'>\n                <span>{{currentSearchConditionName}}</span>\n                <span class=\"el-icon-arrow-down\"\n                      style=\"float:right;line-height:28px;\"></span>\n                <ul v-show='isShowSearchCondition'>\n                  <li v-for=\"(item,index) in articleKeyArr\"\n                      :class=\"[currentSearchIndex==index?'conditionActive':'']\"\n                      @click.stop='choiceSearchCondition(item,index)'>\n                    <span>{{item.name}}</span>\n                  </li>\n                </ul>\n              </div>\n              <div class=\"material_search\">\n                <input type=\"text\"\n                       placeholder=\"请输入标题进行搜索\"\n                       v-model='picKeyWords'\n                       @input='choiceArticleConditionKeywords()'>\n                <img src=\"../../static/img/search.png\"\n                     alt=\"\" />\n              </div>\n              <div class=\"search_button\"\n                   @click.stop='searchConditionMaterial()'>查询</div>\n            </div>\n\n            <div class=\"article_condition\"\n                 v-show='(materialType==\"video\") || (materialType==\"pic\" && singleOrProjectType==\"single\")'>\n              <div class=\"each_search_condition\"\n                   style='left:0;top:10px;'>\n                <span>创建时间：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.duration==0?'search_condition_active':'']\"\n                        @click.stop='choiceArticleCondiTionTime(0)'>全部</span>\n                  <span :class=\"[materialConditionData.duration==1?'search_condition_active':'']\"\n                        @click.stop='choiceArticleCondiTionTime(1)'>今日</span>\n                  <span :class=\"[materialConditionData.duration==7?'search_condition_active':'']\"\n                        @click.stop='choiceArticleCondiTionTime(7)'>7天</span>\n                  <span :class=\"[materialConditionData.duration==30?'search_condition_active':'']\"\n                        @click.stop='choiceArticleCondiTionTime(30)'>30天</span>\n                  <span style=\"margin-right:8px;\">自定义</span>\n                  <el-date-picker v-model=\"searchConditionTime\"\n                                  value-format='yyyy-MM-dd'\n                                  type=\"daterange\"\n                                  range-separator=\"至\"\n                                  start-placeholder=\"开始日期\"\n                                  end-placeholder=\"结束日期\"\n                                  @change=\"selectSelfTime\">\n                  </el-date-picker>\n                </div>\n              </div>\n              <div class=\"each_search_condition\"\n                   style='left:600px;top:10px;'>\n                <span>素材排序：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.sorting==0?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionSort(0)'>创建时间降序</span>\n                  <span :class=\"[materialConditionData.sorting==1?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionSort(1)'>创建时间升序</span>\n                  <span :class=\"[materialConditionData.sorting==2?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionSort(2)'>更新时间降序</span>\n                  <span :class=\"[materialConditionData.sorting==3?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionSort(3)'>更新时间升序</span>\n                </div>\n              </div>\n              <div class=\"each_search_condition\"\n                   style='left:0;top:40px;'>\n                <span>来源类型：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.flag_sys_source=='all'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagSysSource(\"all\")'>全部</span>\n                  <span :class=\"[materialConditionData.flag_sys_source=='news_recover'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagSysSource(\"news_recover\")'>报题素材</span>\n                  <span :class=\"[materialConditionData.flag_sys_source=='mrp_original'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagSysSource(\"mrp_original\")'>素材库原创</span>\n                </div>\n              </div>\n\n              <div class=\"each_search_condition\"\n                   style='left:600px;top:40px;'>\n                <span>与我有关：</span>\n                <div class=\"upload_time_list\"\n                     @click.stop='choiceArticleConditionFlagMyCreate()'>\n                  <div style=\"padding-top:2px;margin-right:5px;\">\n                    <img src=\"../../static/img/clickchoice.png\"\n                         alt=\"\"\n                         v-show='materialConditionData.flag_my_create'>\n                    <img src=\"../../static/img/nochoice.png\"\n                         alt=\"\"\n                         v-show='!materialConditionData.flag_my_create'>\n                  </div>\n                  <span>我上传的</span>\n                </div>\n              </div>\n              <div class=\"each_search_condition\"\n                   style='left:0px;top:70px;'\n                   v-show='materialType==\"video\"'>\n                <span>审核状态：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.flag_audit=='all'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagAudit(\"all\")'>全部</span>\n                  <span :class=\"[materialConditionData.flag_audit=='yes'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagAudit(\"yes\")'>已审核</span>\n                  <span :class=\"[materialConditionData.flag_audit=='no'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagAudit(\"no\")'>未审核</span>\n                </div>\n              </div>\n\n              <div class=\"each_search_condition\"\n                   style='left:600px;top:70px;'\n                   v-show='materialType==\"video\"'>\n                <span>是否签发：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.flag_sign_and_issue=='all'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagIssue(\"all\")'>全部</span>\n                  <span :class=\"[materialConditionData.flag_sign_and_issue=='yes'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagIssue(\"yes\")'>已签发</span>\n                  <span :class=\"[materialConditionData.flag_sign_and_issue=='no'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagIssue(\"no\")'>未签发</span>\n                </div>\n              </div>\n\n              <div class=\"each_search_condition\"\n                   style='left:900px;top:70px;'\n                   v-show='materialType==\"video\"'>\n                <span>是否推送：</span>\n                <div class=\"upload_time_list\">\n                  <span :class=\"[materialConditionData.flag_push=='all'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagPush(\"all\")'>全部</span>\n                  <span :class=\"[materialConditionData.flag_push=='yes'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagPush(\"yes\")'>已推送</span>\n                  <span :class=\"[materialConditionData.flag_push=='no'?'search_condition_active':'']\"\n                        @click.stop='choiceArticleConditionFlagPush(\"no\")'>未推送</span>\n                </div>\n              </div>\n\n            </div>\n\n          </div>\n        </div>\n        <div class=\"material_pic\"\n             v-show='materialType==\"pic\"'>\n          <div class=\"material_pic_content\">\n            <div class=\"material_pic_detail\">\n              <materialpicsingle :isShowSingleTop=true\n                                 :isShowOperate=true></materialpicsingle>\n            </div>\n\n            <div class=\"page_bar\">\n              <pagebar></pagebar>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"material_pic\"\n             v-show='materialType==\"video\"'>\n          <div class=\"material_pic_content\">\n            <div class=\"material_pic_detail\">\n              <materialvideo :isShowOperate=true></materialvideo>\n            </div>\n            <div class=\"page_bar\">\n              <pagebar></pagebar>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"material_pic\"\n             v-show='materialType==\"audio\"'>\n          <div class=\"material_pic_content\">\n            <div class=\"material_pic_detail\">\n              <materialaudio></materialaudio>\n            </div>\n            <div class=\"page_bar\">\n              <pagebar></pagebar>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"material_pic\"\n             v-show='materialType==\"attachment\"'>\n          <div class=\"material_pic_content\">\n            <div class=\"material_pic_detail\">\n              <attachment></attachment>\n            </div>\n            <div class=\"page_bar\">\n              <pagebar></pagebar>\n            </div>\n          </div>\n        </div>\n\n      </div>\n\n      <div class=\"project_pic\"\n           v-show='currentPicType==\"special\"'>\n        <projectpic></projectpic>\n      </div>\n\n      <div class=\"project_pic_detail\"\n           v-show='currentPicType==\"eachimgdetail\"'>\n        <eachpicinfo></eachpicinfo>\n      </div>\n      <div class=\"project_pic\"\n           v-show='currentPicType==\"editorImg\"'>\n        <editorimg></editorimg>\n      </div>\n      <div class=\"project_pic\"\n           v-show='currentPicType==\"videoDetail\