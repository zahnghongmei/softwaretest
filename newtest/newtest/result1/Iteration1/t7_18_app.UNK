ption).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          toastr.success(\'收藏成功\')\n          this.$store.commit(\'SET_VIDEOCOLLECT_DATA\', item)\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n    cancleVideoCollect(item) {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": {\n          "id": item.id,     // 必须字段： 视频素材 ID\n        }\n      }\n      api._post(\'material/video/delete_collection\', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          toastr.success(\'取消收藏成功\')\n          this.$store.commit(\'SET_VIDEOCOLLECT_DATA\', item)\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        console.error(error)\n      })\n\n\n    },\n    pushArticle(item) {\n      //this.$root.eventHub.$emit(\'setPushGuanshanappInfo\', item)\n      this.$root.eventHub.$emit(\'setPushService\', item)\n    }\n\n  }\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only --\x3e\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nul li {\n  list-style: none;\n}\n.materialpic {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  position: relative;\n}\n.materialpic_list_el {\n  flex: 1;\n  width: 100%;\n}\n.materialpic_list {\n  padding-left: 40px;\n  padding-top: 20px;\n  clear: both;\n}\n.materialpic_list li {\n  width: 254px;\n  height: 220px;\n  margin-right: 20px;\n  margin-bottom: 20px;\n  float: left;\n  position: relative