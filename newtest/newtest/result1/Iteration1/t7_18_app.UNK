    "seed": this.seed,            // 必须字段：seed\n        "params": {\n          "id": item.id,   // 必须字段： 图片 ID\n        }\n      }\n      api._post(\'material/picture/delete_collection\', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          toastr.success(\'取消收藏成功\')\n          this.$store.commit(\'SET_COLLECT_PIC\', item)\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n    downloadPic(item) {\n      var option = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "orgId": this.orgId,               // 必须字段：企业 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        "params": {                       // 必须字段： 参数\n          "id": item.id        // 必须字段：视频素材 ID\n        }\n      }\n      var src = api.baseURL + \'material/picture/download\' + \'?token=\' + this.token + \'&request=\' + encodeURI(JSON.stringify(option))\n      downloadFile(src)\n\n    }\n\n\n\n\n  }\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only --\x3e\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nul li {\n  list-style: none;\n}\n.materialpic {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  position: relative;\n}\n.materialpic_top {\n  width: 100%;\n  height: 50px;\n  clear: both;\n  padding-right: 30px;\n}\n.materialpic_pic_type {\n  float: right;\n  height: 50px;\n  display: flex;\n  align-items: center;\n}\n.materialpic_project_pic {\n  height: 50px;\n  display: flex;\n  align-items: center;\n  margin-left: 20px;\n}\n.materialpic_project_circle {\n  width: 16px;\n  height: 16px;\n  background: #4a90e2;\n  border-radius: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: cente