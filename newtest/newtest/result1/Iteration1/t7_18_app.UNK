\n          <span class="keywords"\n                v-for=\'item in currentImgInfo.tags\'>{{item}}</span>\n        </div>\n        <div class="project_pic_info">\n          <span>创建时间：</span>\n          <span>{{currentImgInfo.time_create | formatTime}}</span>\n        </div>\n        <div class="project_pic_info">\n          <span>关&nbsp;&nbsp;键&nbsp;&nbsp;词：</span>\n          <span class="keywords"\n                v-for=\'item in currentImgInfo.keywords\'>{{item}}</span>\n        </div>\n        <div class="project_pic_info">\n          <span>专题概述：</span>\n          <span class="project_descrip">{{currentImgInfo.detail}}</span>\n        </div>\n        <div class="project_pic_operate">\n          <div class="project_pic_editor"\n               @click.stop=\'editorImg()\'\n               v-if=\'currentImgInfo && currentImgInfo.role_item &&  currentImgInfo.role_item.flag_update\'>\n            <span class="iconfont icon-bianji project_pic_editortext"></span>\n            <span class="project_pic_editortext">编辑</span>\n          </div>\n\n          <div class="project_pic_editor"\n               v-if=\'!currentImgInfo.is_collection\'\n               @click.stop=\'collectCurrentPic()\'>\n            <span class="iconfont icon-shoucang project_pic_editortext"></span>\n            <span class="project_pic_editortext">收藏</span>\n          </div>\n          <div class="project_pic_editor"\n               v-if=\'currentImgInfo.is_collection\'\n               @click.stop=\'cancleCurrentPic()\'>\n            <span class="iconfont icon-shoucang project_pic_editortext"\n                  style="color:#FFC500;"></span>\n            <span class="project_pic_editortext">取消收藏</span>\n          </div>\n\n          \x3c!--  <div class="project_pic_editor">\n                    <span class="iconfont icon-fenxiang project_pic_editortext"></span>\n                    <span class="project_pic_editortext">分享</span>\n                </div> --\x3e\n          <div class="project_pic_editor"\n               @click.stop=\'saveAsPic()\'\n               v-if=\'currentImgInfo && currentImgInfo.role_item &&  currentImgInfo.role_item.flag_copy\'>\n            <span class="iconfont icon-keep project_pic_editortext"></span>\n            <span class="project_pic_editortext">另存为</span>\n          </div>\n          <div class="project_pic_editor"\n               @click.stop=\'downloadPic()\'>\n            <span class="iconfont icon-xiazai project_pic_editortext"></span>\n            <span class="project_pic_editortext">下载</span>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class="similar_pic">全部图片</div>\n    <div class="similar_pic_list">\n      <div class="similar_pic_each"\n           :class=\'[curentIndex==index ? "similar_pic_eachactive" : ""]\'\n           @click.stop=\'getCurrentProjectPic($event,item,index)\'\n           v-for=\'(item,index) in eachImgList\'>\n        <div class="similar_pic_eachimg">\n          <img :src="item.url"\n               alt="" />\n        </div>\n      </div>\n      <div class="page_button"\n           @click.stop=\'TurnRightPage\'>\n        <img src="../../static/img/rightarrow.png"\n             alt="" />\n      </div>\n      <div class="page_button page_left_button"\n           @click.stop=\'TurnLeftPage\'>\n        <img src="../../static/img/leftarrow.png"\n             alt="" />\n      </div>\n    </div>\n\n  </div>\n</template>\n  \n<script>\nimport { niceScroller } from \'../../helpers/niceScroller\'\nimport materiallog from \'./materiallog.vue\'\nimport * as api from \'../../api\'\nimport { downloadFile } from \'../../helpers/download\'\nimport { loadingTranstion, closeLoading } from \'../../helpers/loading.js\'\nexport default {\n  name: \'eachpicinfo\',\n  components: {\n    materiallog\n  },\n  data() {\n    return {\n      showmateriallog: false,\n      pageNow: 0,\n      curentIndex: 9999999999999,\n      currentImgInfo: \'\'\n    }\n  },\n  destroyed() {\n    this.$root.eventHub.$off(\'getCurrentImgInfo\')\n  },\n  beforeDestroy() {\n\n  },\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller(\'.project_pic\')\n  },\n  mounted() {\n    niceScroller(\'.project_pic\')\n    this.$root.eventHub.$on(\'getCurrentImgInfo\', (data) => {\n      this.currentImgInfo = data\n    })\n\n  },\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    eachImgList() {\n      return this.$store.state.page.eachImgList\n    },\n    eachImgPageTotal() {\n      return this.$store.state.page.eachImgPageTotal\n    },\n    editorFoldId() {\n      return this.$store.state.articlelist.editorFoldId\n    },\n    eachImgSize() {\n      return this.$store.state.page.eachImgSize\n    },\n    materialType() {\n      return this.$store.state.page.materialType\n    },\n    topicId() {\n      return this.$store.state.page.topicId\n    }\n\n  },\n  methods: {\n    showMaterialLog() {\n      this.showmateriallog = !this.showmateriallog\n    },\n    editorImg() {\n      this.$root.eventHub.$emit(\'getPicType\', \'editorImg\')\n      this.$root.eventHub.$emit(\'editorImgData\', this.currentImgInfo)\n      this.$store.commit(\'SET_EDITOR_IMGINFO\', this.currentImgInfo)\n      loadingTranstion()\n      api._post(\'material/picture/base64_picture?url=\' + this.currentImgInfo.url + \'&token=\' + this.token, \'\').then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          this.$store.commit(\'SET_EDITORBASESIXFOUR_IMGINFO\', resp.data.data)\n          closeLoading()\n        } else {\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n          closeLoading()\n        }\n      }).catch((error) => {\n        toastr.error(error)\n        closeLoading()\n      })\n\n    },\n    getCurrentProjectPic(event, item, index) {\n      this.curentIndex = index\n      this.currentImgInfo = item\n    },\n    TurnRightPage() {\n      if (this.materialType == \'singlePicEach\') {\n        this.pageNow += 1\n        if (this.pageNow < this.eachImgPageTotal) {\n          var singleoption = {\n            "userId": this.userId,        // 必须字段：用户 ID\n            "orgId": this.orgId,          // 必须字段：企业 ID\n            "token": this.token,          // 必须字段：token\n            "seed": this.seed,            // 必须字段：seed\n            "params": {\n              "id_folder": this.editorFoldId,   // 必须字段： 文件夹 ID\n              "page_no": this.pageNow,               // 可选字段：分页的页号，从 0 开始\n              "page_size": this.eachImgSize             // 可选字段：每页条数，从 1 开始，默认 10\n            }\n          }\n          this.$store.dispatch(\'GET_MATERIAL_EACHIMGLIST\', singleoption)\n        } else {\n          this.pageNow = this.eachImgPageTotal - 1\n          toastr.info(\'没有更多数据啦\')\n        }\n      }\n\n      else if (this.materialType == \'projectPicEach\') {\n        this.pageNow += 1\n        if (this.pageNow < this.eachImgPageTotal) {\n          var option = {\n            "userId": this.userId,        // 必须字段：用户 ID\n            "orgId": this.orgId,          // 必须字段：企业 ID\n            "token": this.token,          // 必须字段：token\n            "seed": this.seed,            // 必须字段：seed\n            "params": {\n              "id_topic": this.topicId,     // 必须字段： 专题 ID\n              "page_no": this.pageNow,               // 可选字段：分页的页号，从 0 开始\n              "page_size": this.eachImgSize             // 可选字段：每页条数，从 1 开始，默认 10\n            }\n          }\n          this.$store.dispatch(\'GET_MATERIAL_EACHTOPICIMG\', option)\n        } else {\n          this.pageNow = this.eachImgPageTotal - 1\n          toastr.info(\'没有更多数据啦\')\n        }\n\n      }\n\n\n    },\n    TurnLeftPage() {\n      if (this.materialType == \'singlePicEach\') {\n        this.pageNow -= 1\n        if (this.pageNow >= 0) {\n          var singleoption = {\n            "userId": this.userId,        // 必须字段：用户 ID\n            "orgId": this.orgId,          // 必须字段：企业 ID\n            "token": this.token,          // 必须字段：token\n            "seed": this.seed,            // 必须字段：seed\n            "params": {\n              "id_folder": this.editorFoldId,   // 必须字段： 文件夹 ID\n              "page_no": this.pageNow,               // 可选字段：分页的页号，从 0 开始\n              "page_size": this.eachImgSize             // 可选字段：每页条数，从 1 开始，默认 10\n            }\n          }\n          this.$store.dispatch(\'GET_MATERIAL_EACHIMGLIST\', singleoption)\n        } else {\n          toastr.info(\'已经到第一页啦\')\n          this.pageNow = 0\n        }\n      }\n      else if (this.materialType == \'projectPicEach\') {\n        this.pageNow -= 1\n        if (this.pageNow >= 0) {\n          var option = {\n            "userId": this.userId,        // 必须字段：用户 ID\n            "orgId": this.orgId,          // 必须字段：企业 ID\n            "token": this.token,          // 必须字段：token\n            "seed": this.seed,            // 必须字段：seed\n            "params": {\n              "id_topic": this.topicId,     // 必须字段： 专题 ID\n              "page_no": this.pageNow,               // 可选字段：分页的页号，从 0 开始\n              "page_size": this.eachImgSize             // 可选字段：每页条数，从 1 开始，默认 10\n            }\n          }\n          this.$store.dispatch(\'GET_MATERIAL_EACHTOPICIMG\', option)\n        } else {\n          toastr.info(\'已经到第一页啦\')\n          this.pageNow = 0\n        }\n\n      }\n\n    },\n    saveAsPic() {\n      this.$root.eventHub.$emit(\'setMSaveAs\', this.currentImgInfo, \'picture\')\n    },\n    collectCurrentPic() {\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            /