le\">\n              <div class='share_list_titleinput'>\n                <input type='text'\n                       v-model='channelKeyWords'\n                       @keyup.enter='searchColumnDataList()'\n                       @keyup.delete='getAllColumnDataList()'>\n                <img src='../../static/img/search.png'\n                     art=''\n                     @click.stop='searchColumnDataList()'>\n              </div>\n            </div>\n\n            <el-scrollbar class='share_list_content'>\n              <ul>\n                <pushbar :pushmodels='dataList'></pushbar>\n              </ul>\n            </el-scrollbar>\n\n          </div>\n          <div class=\"share_list_left share_list_right\">\n            <div class=\"share_list_title\">\n              <div class='share_list_titleinput'>\n                <input type='text'\n                       v-model='templateKeyWords'\n                       @keyup.enter='searchTemplateCurrentDataList()'>\n                <img src='../../static/img/search.png'\n                     art=''\n                     @click.stop='searchTemplateCurrentDataList()'>\n              </div>\n            </div>\n\n            <el-scrollbar class='share_list_content'>\n              <ul>\n                <li v-for='item in columnTemplate'>\n                  <div class=\"share_each\">\n                    <span class=\"sharch_name\">{{item.template_name | tempaltename}}</span>\n                    <div @click.stop='choiceColumn($event,item)'>\n                      <div class=\"share_choicepush\"></div>\n                      <div class=\"share_haschoicepush\">\n                        <span class=\"iconfont icon-check\"></span>\n                      </div>\n                    </div>\n                  </div>\n                </li>\n              </ul>\n            </el-scrollbar>\n\n          </div>\n        </div>\n        <div class='has_choice_template'>\n          <span>已选择</span>\n          <el-scrollbar class='ha