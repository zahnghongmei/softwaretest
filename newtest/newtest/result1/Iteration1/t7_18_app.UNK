=\"position:absolute;right:30px;top:18px;\"\n           v-if='isShowEditorAdd'\n           @click.stop='gotoEditor()'>\n        <img src=\"../../static/img/addarticle.png\"\n             alt=\"\"\n             style=\"width:20px;height:20px;\">\n      </div>\n    </div>\n    <div class=\"article_condition\"\n         v-show='downWhichArticleLsit!=\"send\" && isShowArticleCondition'>\n      <div class=\"each_search_condition\">\n        <span>创建时间：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.duration==0?'search_condition_active':'']\"\n                @click.stop='choiceArticleCondiTionTime(0)'>全部</span>\n          <span :class=\"[artilceConditionData.duration==1?'search_condition_active':'']\"\n                @click.stop='choiceArticleCondiTionTime(1)'>今日</span>\n          <span :class=\"[artilceConditionData.duration==7?'search_condition_active':'']\"\n                @click.stop='choiceArticleCondiTionTime(7)'>7天</span>\n          <span :class=\"[artilceConditionData.duration==30?'search_condition_active':'']\"\n                @click.stop='choiceArticleCondiTionTime(30)'>30天</span>\n          <span style=\"margin-right:8px;\">自定义</span>\n          <el-date-picker v-model=\"searchConditionTime\"\n                          value-format='yyyy-MM-dd'\n                          type=\"daterange\"\n                          range-separator=\"至\"\n                          start-placeholder=\"开始日期\"\n                          end-placeholder=\"结束日期\"\n                          @change=\"selectSelfTime\">\n          </el-date-picker>\n        </div>\n      </div>\n      <div class=\"each_search_condition\">\n        <span>稿件排序：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.sorting==0?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionSort(0)'>创建时间降序</span>\n          <span :class=\"[artilceConditionData.sorting==1?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionSort(1)'>创建时间升序</span>\n          <span :class=\"[artilceConditionData.sorting==2?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionSort(2)'>更新时间降序</span>\n          <span :class=\"[artilceConditionData.sorting==3?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionSort(3)'>更新时间升序</span>\n        </div>\n      </div>\n      <div class=\"each_search_condition\">\n        <span>来源类型：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.flag_sys_source=='all'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagSysSource(\"all\")'>全部</span>\n          <span :class=\"[artilceConditionData.flag_sys_source=='news_recover'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagSysSource(\"news_recover\")'>报题稿件</span>\n          <span :class=\"[artilceConditionData.flag_sys_source=='mrp_original'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagSysSource(\"mrp_original\")'>稿件库原创</span>\n        </div>\n      </div>\n      <div class=\"each_search_condition\">\n        <span>是否推送：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.flag_push=='all'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagPush(\"all\")'>全部</span>\n          <span :class=\"[artilceConditionData.flag_push=='yes'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagPush(\"yes\")'>已推送</span>\n          <span :class=\"[artilceConditionData.flag_push=='no'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagPush(\"no\")'>未推送</span>\n        </div>\n      </div>\n      <div class=\"each_search_condition\">\n        <span>审核状态：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.flag_audit=='all'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagAudit(\"all\")'>全部</span>\n          <span :class=\"[artilceConditionData.flag_audit=='yes'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagAudit(\"yes\")'>已审核</span>\n          <span :class=\"[artilceConditionData.flag_audit=='no'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagAudit(\"no\")'>未审核</span>\n        </div>\n      </div>\n      <div class=\"each_search_condition\">\n        <span>是否采用：</span>\n        <div class=\"upload_time_list\">\n          <span :class=\"[artilceConditionData.flag_use=='all'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagUse(\"all\")'>全部</span>\n          <span :class=\"[artilceConditionData.flag_use=='yes'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagUse(\"yes\")'>已采用</span>\n          <span :class=\"[artilceConditionData.flag_use=='no'?'search_condition_active':'']\"\n                @click.stop='choiceArticleConditionFlagUse(\"no\")'>未采用</span>\n        </div>\n      </div>\n\n      <div class=\"each_search_condition\">\n        <span>与我有关：</span>\n        <div class=\"upload_time_list\"\n             @click.stop='choiceArticleConditionFlagMyCreate()'>\n          <div style=\"padding-top:2px;margin-right:5px;\">\n            <img src=\"../../static/img/clickchoice.png\"\n                 alt=\"\"\n                 v-show='artilceConditionData.flag_my_create'>\n            <img src=\"../../static/img/nochoice.png\"\n                 alt=\"\"\n                 v-show='!artilceConditionData.flag_my_create'>\n          </div>\n          <span>我创建的</span>\n        </div>\n      </div>\n    </div>\n    <div class=\"search_click_button\">\n      <div class=\"search_button\"\n           @click.stop='sortChoice()'>查询</div>\n      <div class=\"search_button_icon\"\n           @click.stop='showArticleSearchCondition()'\n           v-show='downWhichArticleLsit!=\"send\"'>\n        <span class=\"el-icon-arrow-down\"\n              v-show='!isShowArticleCondition'></span>\n        <span class=\"el-icon-arrow-up\"\n              v-show='isShowArticleCondition'></span>\n      </div>\n    </div>\n\n    <div class=\"new_data\">\n      <div class=\"news_total\">\n        <span>文章总数：</span>\n        <span>{{articlePageTotal}}</span>\n      </div>\n      <div class=\"news_total news_today\">\n        <span>今日新增：</span>\n        <span>{{updateTodayDataNum}}</span>\n      </div>\n    </div>\n\n    <div id='news_list_pageid'\n         class=\"news_list_page\"\n         @scroll.stop='getNextArticle($event)'\n         :class=\"[isShowArticleCondition?'min_list_height':'max_list_height']\">\n      <articlelist :isShowPath='isShowPathbool'\n                   :dataList='articleList'></articlelist>\n    </div>\n\n  </div>\n</template>\n  \n<script>\nimport articlelist from './articlelist.vue'\nimport { niceScroller } from '../../helpers/niceScroller'\nexport default {\n  name: 'mediacontentleft',\n  components: {\n    articlelist\n  },\n  data() {\n    return {\n      isSortCreateTime: true,\n      isShowSortUl: false,\n      searchArticleKey: '',\n      currentSearchIndex: 0,\n      isShowSearchCondition: false,\n      currentSearchConditionName: '全部',\n      searchConditionTime: '',\n      isShowArticleCondition: false,\n      articleKeyArr: [{ name: '全部', type: 'all' }, { name: '标题', type: 'title' }, { name: '正文', type: 'content' }, { name: '关键词', type: 'keywords' }, { name: '摘要', type: 'summary' }]\n    }\n  },\n  destroyed() {\n    this.$root.eventHub.$off('closeTimeBox')\n  },\n  beforeDestroy() {\n\n  },\n  mounted() {\n    niceScroller('.news_list_page')\n    this.$store.commit('SET_ARTICLE_SORT', 'c')\n    this.$store.commit('SET_CLEAR_PAGE')\n    this.$root.eventHub.$on('closeTimeBox', (data) => {\n      this.isShowSortUl = false\n      this.isShowSearchCondition = false\n    })\n  },\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller('.news_list_page')\n  },\n  computed: {\n    articleList() {\n      return this.$store.state.articlelist.articleList\n    },\n    articlePageTotal() {\n      return this.$store.state.articlelist.articlePageTotal\n    },\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    sort() {\n      return this.$store.state.articlelist.sort\n    },\n    api_name() {\n      return this.$store.state.articlelist.api_name\n    },\n    order() {\n      return this.$store.state.articlelist.order\n    },\n    page_no() {\n      return this.$store.state.articlelist.page_no\n    },\n    page_size() {\n      return this.$store.state.articlelist.page_size\n    },\n    repositoryId() {\n      return this.$store.state.repo.repositoryId\n    },\n    allArticleDataNum() {\n      return this.$store.state.articlelist.allArticleDataNum\n    },\n    updateTodayDataNum() {\n      return this.$store.state.articlelist.updateTodayDataNum\n    },\n    editorFoldId() {\n      return this.$store.state.articlelist.editorFoldId\n    },\n    downWhichArticleLsit() {\n      return this.$store.state.articlelist.downWhichArticleLsit\n    },\n    isShowEditorAdd() {\n      return this.$store.state.repo.isShowEditorAdd\n    },\n    isShowPathbool() {\n      if (this.downWhichArticleLsit == 'collect' || this.downWhichArticleLsit == 'mesharetoother' || this.downWhichArticleLsit == 'sharetome' || this.downWhichArticleLsit == 'send') {\n        return true\n      } else {\n        return false\n      }\n    },\n    artilceConditionData() {\n      return this.$store.state.articlelist.artilceConditionData\n    },\n    folderFirstData() {\n      return this.$store.state.repo.folderFirstData\n    }\n  },\n  methods: {\n    sortChoice(type, name) {\n      this.isSortCreateTime = type\n      this.isShowSortUl = false\n      this.$store.commit('SET_ARTICLE_SORT', name)\n      $('.news_list_page').scrollTop('0')\n      if (this.downWhichArticleLsit == 'normal') {\n        this.getFirstArticle()\n      } else if (this.downWhichArticleLsit == 'collect') {\n        this.getCollectFirstArticleData()\n      } else if (this.downWhichArticleLsit == 'mesharetoother') {\n        this.getMyShareFirstArticleData()\n      } else if (this.downWhichArticleLsit == 'sharetome') {\n        this.getShareToMeFirstArticleData()\n      } else if (this.downWhichArticleLsit == 'send') {\n        this.getPushFirstArticleData()\n      }\n\n    },\n    showSortUl() {\n      this.isShowSortUl = !this.isShowSortUl\n    },\n    gotoEditor() {\n      this.$store.commit('SET_EDITORDATA_ITEM', '')\n      this.$store.commit('SET_CLEARAI_COMMENTS')\n      this.$store.commit('SET_COMMENTS_ID', '')\n      this.$store.commit('SET_CLEARAI_LOGOPERATE')\n      this.$store.commit('SET_CLEARAI_HISTORYRECORD')\n      this.$store.commit('SET_CLEARAI_AUDITLOG')\n      localStorage.setItem(\"editorManuscriptInfo\", '')\n      if (this.editorFoldId == '') {\n        toastr.error('请选择文件夹')\n      } else {\n        this.$store.commit('SET_SAVE_CURRENTROUTER', this.$route.path)\n        this.$router.replace('/editormain/editorcontent')\n        this.$store.commit('SET_EDITORBUTTON_ROLE', true)\n      }\n\n\n    },\n    getNextArticle(event) {\n      var relHeight = document.getElementById('news_list_pageid').scrollHeight\n      var divHeight = document.getElementById('news_list_pageid').clientHeight\n      var scrollTop = document.getElementById('news_list_pageid').scrollTop\n      if (this.articleList.length > 0) {\n        if (this.downWhichArticleLsit == 'normal') {\n          var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n          articleObjOption.id_folder = this.repositoryId\n          articleObjOption.page_no = this.page_no\n          articleObjOption.page_size = this.page_size\n\n          var articleOption = {\n            \"userId\": this.userId,                 // 必须字段：用户 id\n            \"token\": this.token,                    // 必须字段： token\n            \"seed\": this.seed,                      // 必须字段： seed\n            'orgId': this.orgId,\n            \"params\": articleObjOption\n          }\n          if (scrollTop + divHeight == relHeight) {\n            var totalPage = Math.floor(this.articlePageTotal / this.page_size)\n            if (this.page_no < totalPage || this.page_no == totalPage) {\n              this.$store.dispatch('GET_ARTICLELIST_NEXTDATALIST', articleOption)\n            }\n          }\n        }\n        else if (this.downWhichArticleLsit == 'collect') {\n          var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n          articleObjOption.page_no = this.page_no\n          articleObjOption.page_size = this.page_size\n          var collectOption = {\n            \"userId\": this.userId,             // 必须字段：用户 id\n            \"token\": this.token,                // 必须字段： token\n            \"seed\": this.token,                  // 必须字段： seed\n            'orgId': this.orgId,\n            \"params\": articleObjOption\n          }\n          if (scrollTop + divHeight == relHeight) {\n            var totalPage = Math.floor(this.articlePageTotal / this.page_size)\n            if (this.page_no < totalPage || this.page_no == totalPage) {\n              this.$store.dispatch('GET_COLLECT_NEXTDATALIST', collectOption)\n            }\n          }\n\n        }\n        else if (this.downWhichArticleLsit == 'mesharetoother') {\n          var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n          articleObjOption.page_no = this.page_no\n          articleObjOption.page_size = this.page_size\n          articleObjOption.direction = 'my_share'\n          var shareoption = {\n            \"userId\": this.userId,             // 必须字段：用户 id\n            \"token\": this.token,                // 必须字段： token\n            \"seed\": this.seed,                  // 必须字段： seed\n            'orgId': this.orgId,\n            \"params\": articleObjOption\n          }\n          if (scrollTop + divHeight == relHeight) {\n            var totalPage = Math.floor(this.articlePageTotal / this.page_size)\n            if (this.page_no < totalPage || this.page_no == totalPage) {\n              this.$store.dispatch('GET_MYSHARE_NEXTDATALIST', shareoption)\n            }\n          }\n\n        }\n        else if (this.downWhichArticleLsit == 'sharetome') {\n          var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n          articleObjOption.page_no = this.page_no\n          articleObjOption.page_size = this.page_size\n          articleObjOption.direction = 'be_share'\n          var shareoption = {\n            \"userId\": this.userId,             // 必须字段：用户 id\n            \"token\": this.token,                // 必须字段： token\n            \"seed\": this.seed,                  // 必须字段： seed\n            'orgId': this.orgId,\n            \"params\": articleObjOption\n          }\n          if (scrollTop + divHeight == relHeight) {\n            var totalPage = Math.floor(this.articlePageTotal / this.page_size)\n            if (this.page_no < totalPage || this.page_no == totalPage) {\n              this.$store.dispatch('GET_MYSHARE_NEXTDATALIST', shareoption)\n            }\n          }\n\n        }\n        else if (this.downWhichArticleLsit == 'send') {\n          var pushoption = {\n            \"userId\": this.userId,             // 必须字段：用户 id\n            \"token\": this.token,                // 必须字段： token\n            \"seed\": this.seed,                  // 必须字段： seed\n            'orgId': this.orgId,\n            \"params\": {                       // 必须字段： 请求参数\n              \"keywords\": this.searchArticleKey,          // 可选字段： 关键词\n              \"page_no\": this.page_no,                     // 可选字段： 分页，页号，从 0 开始， 默认 0\n              \"page_size\": this.page_size,                  // 可选字段： 分页，返回数量，默认 10，最大 100\n              \"sort\": this.sort,                   // 可选字段： 排序字段 c 或 u， c 为创建时间， u  为更新时间\n              \"order\": this.order              // 可选字段： 排序方式，倒序 desc, 升序 asc, 默认 desc，以更新时间排序\n            }\n          }\n          if (scrollTop + divHeight == relHeight) {\n            var totalPage = Math.floor(this.articlePageTotal / this.page_size)\n            if (this.page_no < totalPage || this.page_no == totalPage) {\n              this.$store.dispatch('GET_PUSH_NEXTDATALIST', pushoption)\n            }\n          }\n\n        }\n      }\n\n\n\n\n\n\n    },\n\n\n    //获取正常文章第一页数据\n    getFirstArticle() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n      var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n      articleObjOption.id_folder = this.repositoryId\n      articleObjOption.page_no = 0\n      articleObjOption.page_size = this.page_size\n      var articleOption = {\n        \"userId\": this.userId,                 // 必须字段：用户 id\n        \"token\": this.token,                    // 必须字段： token\n        \"seed\": this.seed,                      // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": articleObjOption\n      }\n      this.$store.dispatch('GET_ARTICLELIST_FIRSTDATALIST', articleOption)\n    },\n    //获取收藏文章第一页数据\n    getCollectFirstArticleData() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n\n      var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n      articleObjOption.page_no = 0\n      articleObjOption.page_size = this.page_size\n      var option = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.token,                  // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": articleObjOption\n      }\n      this.$store.dispatch('GET_COLLECT_FIRSTDATALIST', option)\n\n    },\n    //获取我分享的第一页数据\n    getMyShareFirstArticleData() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n      var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n      articleObjOption.page_no = 0\n      articleObjOption.page_size = this.page_size\n      articleObjOption.direction = 'my_share'\n      var option = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.seed,                  // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": articleObjOption\n      }\n      this.$store.dispatch('GET_MYSHARE_FIRSTDATALIST', option)\n    },\n    //获取分享给我的第一页数据\n    getShareToMeFirstArticleData() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n      var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n      articleObjOption.page_no = 0\n      articleObjOption.page_size = this.page_size\n      articleObjOption.direction = 'be_share'\n      var option = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.seed,                  // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": articleObjOption\n      }\n      this.$store.dispatch('GET_MYSHARE_FIRSTDATALIST', option)\n    },\n    //获取推送第一页数据\n    getPushFirstArticleData() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n      var option = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.seed,                  // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": {                       // 必须字段： 请求参数\n          \"keywords\": this.searchArticleKey,          // 可选字段： 关键词\n          \"page_no\": 0,                     // 可选字段： 分页，页号，从 0 开始， 默认 0\n          \"page_size\": this.page_size,                  // 可选字段： 分页，返回数量，默认 10，最大 100\n          \"sort\": this.sort,                   // 可选字段： 排序字段 c 或 u， c 为创建时间， u  为更新时间\n          \"order\": this.order              // 可选字段： 排序方式，倒序 desc, 升序 asc, 默认 desc，以更新时间排序\n        }\n      }\n      this.$store.dispatch('GET_PUSH_FIRSTDATALIST', option)\n\n    },\n    choiceSearchCondition(item, index) {\n      this.currentSearchConditionName = item.name\n      this.currentSearchIndex = index\n      this.isShowSearchCondition = false\n      var option = {\n        key: 'match_field',\n        value: item.type\n      }\n      this.$store.commit('SET_ARTICLE_CONDITIONOPTION', option)\n    },\n    showSearcyTypeCondition() {\n      this.isShowSearchCondition = !this.isShowSearchCondition\n    },\n    showArticleSearchCondition() {\n      this.isShowArticleCondition = !this.isShowArticleCondition\n    },\n    choiceArticleCondiTionTime(value) {\n      this.searchConditionTime = ''\n      var option = {\n        key: 'duration',\n        value: value\n      }\n      this.$store.commit('SET_ARTICLE_CONDITIONOPTION', option)\n    },\n    selectSelfTime(value) {\n      if (value) {\n        var option = {\n          key: 'duration',\n          value: 31\n        }\n        this.$store.commit('SET_ARTICLE_CONDITIONOPTION', option)\n        var optionTimeStart = {\n          key: \"time_start\",\n          value: value[0]\n        }\n        this.$store.commit('SET_ARTICLE_CONDITIONOPTION', optionTimeStart)\n\n        var optionTimeStop = {\n          key: \"time_stop\",\n          value: value[1]\n        }\n        this.$store.commit('SE