     <li @click.stop='choiceCurrentResponsitory(\"稿件库\",\"article\")'>稿件库</li>\n            <li @click.stop='choiceCurrentResponsitory(\"素材库\",\"material\")'>素材库</li>\n          </ul>\n        </div>\n      </div>\n    </div>\n    <el-scrollbar class='ul_list'>\n      <ul>\n        <librarytree :models=\"repoFolderData\"></librarytree>\n      </ul>\n    </el-scrollbar>\n    <div class=\"upload_material\"\n         v-if='isShowDownload'\n         @click.stop='showUploadStatus()'>\n      <div class=\"upload_cicle\">\n        <span class=\"iconfont icon-tianjia\"></span>\n      </div>\n      <div class=\"upload_text\">上传素材</div>\n      <ul v-show='showUploadUl'>\n        <li @click.stop='uploadMaterial(\"project\")'>专题</li>\n        <li @click.stop='uploadMaterial(\"picture\")'>图片</li>\n        <li @click.stop='uploadMaterial(\"video\")'>视频</li>\n        <li @click.stop='uploadMaterial(\"audio\")'>音频</li>\n        <li @click.stop='uploadMaterial(\"attachment\")'>附件</li>\n      </ul>\n    </div>\n    <div class=\"upload_material\"\n         v-if='!isShowDownload && isShowEditorAdd'\n         @click.stop='uploadManuscript()'>\n      <div class=\"upload_cicle\">\n        <span class=\"iconfont icon-tianjia\"></span>\n      </div>\n      <div class=\"upload_text\">上传稿件</div>\n    </div>\n\n  </div>\n</template>\n<script>\nimport librarytree from './librarytree.vue'\nimport { niceScroller } from '../../helpers/niceScroller'\nimport * as api from '../../api'\nimport * as locCookie from '../../helpers/cookieHandler'\nexport default {\n  name: 'wrapperleft',\n  components: {\n    librarytree\n  },\n  props: ['isShowDownload'],\n  data() {\n    return {\n      libraryName: '稿件库',\n      Typelibrary: 'article',\n      showRepository: false,\n      showUploadUl: false\n\n    }\n  },\n  beforeDestroy() {\n\n  },\n  destroyed() {\n    this.$root.eventHub.$off('closeUploadBox')\n  },\n  updated() {\n\n  },\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    repoData() {\n      return this.$store.state.repo.modelManagerDataList\n    },\n    repoFolderData() {\n      return this.$store.state.repo.repoFolderData\n    },\n    repoType() {\n      return this.$store.state.repo.repoType\n    },\n    editorFoldId() {\n      return this.$store.state.articlelist.editorFoldId\n    },\n\n    order() {\n      return this.$store.state.articlelist.order\n    },\n    page_no() {\n      return this.$store.state.articlelist.page_no\n    },\n    page_size() {\n      return this.$store.state.articlelist.page_size\n    },\n    sort() {\n      return this.$store.state.articlelist.sort\n    },\n    api_name() {\n      return this.$store.state.articlelist.api_name\n    },\n    repoObj() {\n      return this.$store.state.repo.repoObj\n    },\n    entityMap() {\n      return this.$store.state.repo.entityMap\n    },\n    currentMaterialType() {\n      return this.$store.state.page.currentMaterialType\n    },\n    artilceConditionData() {\n      return this.$store.state.articlelist.artilceConditionData\n    },\n    pageSize() {\n      return this.$store.state.page.pageSize\n    },\n    materialConditionData() {\n      return this.$store.state.page.materialConditionData\n    },\n    folderFirstData() {\n      return this.$store.state.repo.folderFirstData\n    },\n    isShowEditorAdd() {\n      return this.$store.state.repo.isShowEditorAdd\n    }\n\n  },\n  mounted() {\n    this.$root.eventHub.$on('closeUploadBox', (data) => {\n      this.showUploadUl = false\n      this.showRepository = false\n    })\n\n\n    if (this.$route.path == '/material/materialcontent') {\n      this.$store.commit('SET_SAVEREPOTYPE_DATA', 'material')\n      this.libraryName = '素材库'\n      this.Typelibrary = 'material'\n    } else {\n      this.$store.commit('SET_SAVEREPOTYPE_DATA', 'article')\n      this.libraryName = '稿件库'\n      this.Typelibrary = 'article'\n      this.$store.commit('SET_CLEARREPOANDFOLD_ID')\n    }\n    // this.getRepo()\n    // this.getMaterialData()\n    // this.getWaterMark()\n\n    this.getCodeAndRepo()\n  },\n  methods: {\n    getCodeAndRepo() {\n      var codenow = this.getUrlCode('token')\n      var id_folder = this.getUrlCode('id_folder')\n      var id_article = this.getUrlCode('id_article')\n      if (codenow) {\n        locCookie.setCookie('token', codenow)\n      }\n\n      if (codenow) {\n        api._get('user/get_user_info', '').then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            if (res.data.data.userId) {\n              locCookie.setCookie('userId', res.data.data.userId)\n            } else {\n              locCookie.setCookie('userId', '')\n            }\n            if (res.data.data.seed) {\n              locCookie.setCookie('seed', res.data.data.seed)\n            } else {\n              locCookie.setCookie('seed', '')\n            }\n\n            if (res.data.data.user.user_name) {\n              locCookie.setCookie('username', res.data.data.user.user_name)\n            } else {\n              locCookie.setCookie('username', '')\n            }\n            if (res.data.data.user.head_img) {\n              locCookie.setCookie('headimg', res.data.data.user.head_img)\n            } else {\n              locCookie.setCookie('headimg', '')\n            }\n            if (res.data.data.orgId) {\n              locCookie.setCookie('orgId', res.data.data.orgId)\n            } else {\n              locCookie.setCookie('orgId', '')\n            }\n            if (res.data.data.token) {\n              locCookie.setCookie('token', res.data.data.token)\n            } else {\n              locCookie.setCookie('token', '')\n            }\n            if (res.data.data.orgRoot.org_name) {\n              locCookie.setCookie('orgName', res.data.data.orgRoot.org_name)\n            } else {\n              locCookie.setCookie('orgName', '')\n            }\n\n\n            var option = {\n              token: res.data.data.token,\n              userId: res.data.data.userId,\n              seed: res.data.data.seed,\n              orgId: res.data.data.orgId,\n              username: res.data.data.user.user_name,\n              headimg: res.data.data.user.head_img,\n              orgName: res.data.data.orgRoot.org_name\n            }\n\n            this.$store.commit('SET_TOKEN_DATA', option)\n            if (this.token) {\n              this.getRepo()\n              this.getMaterialData()\n              if (id_folder) {\n                this.getArticleDataListFirst(id_folder)\n              }\n              if (id_article) {\n                this.getArticleDetail(id_article)\n              }\n\n            }\n\n          } else {\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          console.error(error)\n        })\n      } else {\n        if (this.token) {\n          this.getRepo()\n          this.getMaterialData()\n          if (id_folder) {\n            this.getArticleDataListFirst(id_folder)\n          }\n          if (id_article) {\n            this.getArticleDetail(id_article)\n          }\n\n        }\n      }\n\n    },\n    getParam(name, url) {\n      if (!url) url = location.href;\n      name = name.replace(/[\\[]/, \"\\\\\\[\").replace(/[\\]]/, \"\\\\\\]\");\n      var regexS = \"[\\\\?&]\" + name + \"=([^&#]*)\";\n      var regex = new RegExp(regexS);\n      var results = regex.exec(url);\n      return results == null ? null : results[1];\n    },\n\n    getUrlCode(value) {\n      let currentUrl = window.location.href\n      let code = this.getParam(value, currentUrl)\n      if (value == 'token' && code) {\n        localStorage.setItem('token', code)\n      }\n      return code\n    },\n\n    //获取文章详情\n    getArticleDetail(id) {\n      var option = {\n        \"userId\": this.userId,                  // 必须字段：用户 id\n        \"token\": this.token,                     // 必须字段： token\n        \"seed\": this.seed,                       // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": {                            // 必须字段： 创建文章参数\n          \"api_name\": \"get_by_article_id\",   // 必须固定值字段： 值： get_by_article_id\n          \"id\": id,               // 必须字段： 文章（稿件） id\n        }\n      }\n      api._post('article/get_article', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          if (res.data.data && res.data.data.total && res.data.data.total == 1) {\n            this.$root.eventHub.$emit('articleInfo', res.data.data.list[0])\n          }\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n\n    getArticleDataListFirst(id) {\n      if (id) {\n        var sendCurrentData = {\n          \"userId\": this.userId,\n          \"token\": this.token,\n          \"seed\": this.seed,\n          'orgId': this.orgId,\n          \"params\": {\n            \"api_name\": 'by_id_folder',\n            \"id\": id\n          }\n        }\n\n        this.$store.dispatch('GET_ARTICLE_ALLTOTALANDUPDATE', sendCurrentData)\n\n        var keywordsoption = {\n          key: 'keywords',\n          value: ''\n        }\n        this.$store.commit('SET_ARTICLE_CONDITIONOPTION', keywordsoption)\n        var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n        articleObjOption.id_folder = id\n        articleObjOption.page_no = 0\n        articleObjOption.page_size = this.page_size\n\n        var articleOption = {\n          \"userId\": this.userId,                 // 必须字段：用户 id\n          \"token\": this.token,                    // 必须字段： token\n          \"seed\": this.seed,                      // 必须字段： seed\n          'orgId': this.orgId,\n          \"params\": articleObjOption\n        }\n        this.$store.dispatch('GET_ARTICLELIST_FIRSTDATALIST', articleOption)\n\n      }\n    },\n    //获取水印列表\n    getWaterMark() {\n      var option = {\n        \"userId\": this.userId,        // 必须字段：用户 ID\n        \"orgId\": this.orgId,          // 必须字段：企业 ID\n        \"token\": this.token,          // 必须字段：token\n        \"seed\": this.seed,            // 必须字段：seed\n        \"params\": {                 // 必须字段： 参数\n        }\n      }\n      this.$store.dispatch('GET_WATERMARKLIST_DATA', option)\n\n    },\n    choiceRepository() {\n      this.showRepository = !this.showRepository\n    },\n    choiceCurrentResponsitory(name, type) {\n      this.libraryName = name\n      this.showRepository = false\n      this.Typelibrary = type\n      this.$store.commit('SET_SAVEREPOTYPE_DATA', type)\n      this.clearAllData()\n      this.getRepo()\n      if (type == 'material') {\n        this.$router.replace('/material/materialcontent')\n      } else {\n        this.$router.replace('/')\n      }\n\n    },\n    //先清空数据\n    clearAllData() {\n      this.$store.commit('SET_CLEAR_PAGE')\n      this.$store.commit('SET_CLEAR_ARTICLELIST')\n      this.$store.commit('SET_ARTICLE_DETAIL', '')\n      this.$store.commit('SET_KEY_WORDS', '')\n      this.$store.commit('SET_CLEAR_TOTALNUM')\n      this.$store.commit('SET_CLEARAI_COMMENTS')\n      this.$store.commit('SET_COMMENTS_ID', '')\n      this.$store.commit('SET_WHICHDOWN_ARTICLElLIST', 'normal')\n      this.$store.commit('SET_CLEARAI_LOGOPERATE')\n      this.$store.commit('SET_CLEARAI_HISTORYRECORD')\n      this.$store.commit('SET_CLEARREPOANDFOLD_ID')\n      this.$store.commit('SET_FOLDERFIRST_DATA', '')\n      this.$store.commit('SET_REPO_OBJDATA', '')\n      this.$store.commit('SET_PAGE_NOW', 1)\n      this.$store.commit('SET_CLEARPICLIST_DATA')\n    },\n    getRepo() {\n      this.$store.commit('SET_CLEAR_REPODATA')\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数\n          \"flag_type\": this.Typelibrary,         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch('GET_REPOFODLER_DATA', option)\n    },\n\n    getMaterialData() {\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数\n          \"flag_type\": 'material',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch('GET_MATERIAL_DATA', option)\n    },\n    uploadMaterial(name) {\n      this.$root.eventHub.$emit('setUploadPic', name)\n      this.showUploadUl = false\n    },\n    showUploadStatus() {\n      this.showUploadUl = !this.showUploadUl\n    },\n    uploadManuscript() {\n      this.$store.commit('SET_EDITORDATA_ITEM', '')\n      this.$store.commit('SET_CLEARAI_COMMENTS')\n      this.$store.commit('SET_COMMENTS_ID', '')\n      this.$store.commit('SET_CLEARAI_LOGOPERATE')\n      this.$store.commit('SET_CLEARAI_HISTORYRECORD')\n      this.$store.commit('SET_CLEARAI_AUDITLOG')\n      localStorage.setItem(\"editorManuscriptInfo\", '')\n\n      if (this.editorFoldId == '') {\n        toastr.error('请选择文件夹')\n      } else {\n        this.$store.commit('SET_SAVE_CURRENTROUTER', this.$route.path)\n        this.$router.replace('/editormain/editorcontent')\n        this.$store.commit('SET_EDITORBUTTON_ROLE', true)\n      }\n    },\n    getMaterialRepoAndFolderInfo(item) {\n      this.$store.commit('SET_PAGE_NOW', 1)\n      this.$store.commit('SET_CLEARPICLIST_DATA')\n      this.$root.eventHub.$emit('setToPic', '')\n      this.$root.eventHub.$emit('clearKeyWords', '')\n      this.$store.commit('SET_MATERIAL_TYPE', 'pic')\n      this.$root.eventHub.$emit('getPicType', 'single')\n      this.$root.eventHub.$emit('clearEditorPicData', '')\n      this.$store.commit('SET_EDITOR_IMGINFO', '')\n      this.$store.commit('SET_ISSHOWINNER_IMG', true)\n      if (this.currentMaterialType == 'pic') {\n        this.$store.commit('SET_ORGRIANLARTICLE_CONDITIONOPTION')\n        var materialObjOption = JSON.parse(JSON.stringify(this.materialConditionData))\n        materialObjOption.id_folder = item.id\n        materialObjOption.page_no = 0\n        materialObjOption.page_size = this.pageSize\n        var singleoption = {\n          \"userId\": this.userId,        // 必须字段：用户 ID\n          \"orgId\": this.orgId,          // 必须字段：企业 ID\n          \"token\": this.token,          // 必须字段：token\n          \"seed\": this.seed,            // 必须字段：seed\n          \"params\": materialObjOption\n        }\n        this.$root.eventHub.$emit('setPicType', '')\n        this.$store.dispatch('GET_MATERIAL_SINGLEPICLIST', singleoption)\n      }\n    },\n    getArticleRepoAndFolderinfo(item) {\n      if (item.role_item.flag_create) {\n        this.$store.commit('SET_ISSHOWEDITOR_ADDICON', true)\n      } else {\n        this.$store.commit('SET_ISSHOWEDITOR_ADDICON', false)\n      }\n      //获取文章总数和今日更新\n      var sendCurrentData = {\n        \"userId\": this.userId,\n        \"token\": this.token,\n        \"seed\": this.seed,\n        'orgId': this.orgId,\n        \"params\": {\n          \"api_name\": 'by_id_folder',\n          \"id\": item.id\n        }\n      }\n\n      this.$store.dispatch('GET_ARTICLE_ALLTOTALANDUPDATE', sendCurrentData)\n      //获取文章第一页数据\n      var keywordsoption = {\n        key: 'keywords',\n        value: ''\n      }\n      this.$store.commit('SET_ARTICLE_CONDITIONOPTION', keywordsoption)\n      var articleObjOption = JSON.parse(JSON.stringify(this.artilceConditionData))\n    