idth: 1540px) {\n}\n',"",{version:3,sources:["/dockerdata/jenkins/.jenkins/workspace/MRP_Words_Web/src/components/sharearticle/src/components/sharearticle/sharearticleinfo.vue"],names:[],mappings:";AA8TA;EACA,+BAAA;UAAA,uBAAA;CACA;AACA;;EAEA,iBAAA;CACA;AACA;EACA,YAAA;EACA,aAAA;EACA,oBAAA;CACA;AACA;EACA,YAAA;EACA,aAAA;EACA,yCAAA;EACA,4DAAA;UAAA,oDAAA;EACA,yCAAA;EACA,kBAAA;EACA,mBAAA;EACA,gBAAA;CACA;AACA;EACA,eAAA;EACA,gBAAA;EACA,oCAAA;CACA;AAEA;EACA,YAAA;EACA,0BAAA;EACA,eAAA;CACA;AACA;EACA,WAAA;EACA,YAAA;EACA,gBAAA;EACA,kBAAA;EACA,yCAAA;EACA,4DAAA;UAAA,oDAAA;EACA,yCAAA;CACA;AACA;CACA",file:"sharearticleinfo.vue",sourcesContent:["<template>\n  <div class=\"audit\"\n       @click.stop='closecommentsbox()'>\n    <div class=\"audit_top\"\n         @click.stop='goToIndex()'>\n      <span class=\"iconfont icon-fanhui\"></span>\n      <span>返回</span>\n    </div>\n    <div class=\"audit_box\">\n      <div class=\"audit_box_content\">\n        <mediacontentright :articleObj='shareArticleObj'></mediacontentright>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from '../../api'\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport Vue from 'vue'\nimport { playVideo } from '../../helpers/jplayer.js'\nimport * as locCookie from '../../helpers/cookieHandler'\nimport mediacontentright from '../wrapperright/mediacontentright.vue'\nimport commentlog from '../../mixins/commentlog.js'\nexport default {\n  mixins: [commentlog],\n  components: {\n    mediacontentright\n  },\n\n  created() {\n    var currentoption = this.getUrlCode()\n    if (currentoption.token && currentoption.msg_type && currentoption.id_item) {\n      this.$store.commit('SET_WHICHDOWN_ARTICLElLIST', 'sharetome')\n      this.getCodeAndRepo(currentoption)\n    }\n\n\n  },\n  updated() {\n\n  },\n\n  mounted() {\n\n  },\n\n  destroyed() {\n\n  },\n\n  data() {\n    return {\n      articleDetail: ''\n    }\n  },\n\n  updated() {\n\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    shareArticleObj() {\n      return this.$store.state.articledetail.shareArticleObj\n    }\n\n\n\n\n  },\n\n  methods: {\n    getCodeAndRepo(msgoption) {\n      if (msgoption.token) {\n        api._get('user/get_user_info', '').then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            if (res.data.data.userId) {\n              locCookie.setCookie('userId', res.data.data.userId)\n            } else {\n              locCookie.setCookie('userId', '')\n            }\n            if (res.data.data.seed) {\n              locCookie.setCookie('seed', res.data.data.seed)\n            } else {\n              locCookie.setCookie('seed', '')\n            }\n\n            if (res.data.data.user.user_name) {\n              locCookie.setCookie('username', res.data.data.user.user_name)\n            } else {\n              locCookie.setCookie('username', '')\n            }\n            if (res.data.data.user.head_img) {\n              locCookie.setCookie('headimg', res.data.data.user.head_img)\n            } else {\n              locCookie.setCookie('headimg', '')\n            }\n            if (res.data.data.orgId) {\n              locCookie.setCookie('orgId', res.data.data.orgId)\n            } else {\n              locCookie.setCookie('orgId', '')\n            }\n            if (res.data.data.token) {\n              locCookie.setCookie('token', res.data.data.token)\n            } else {\n              locCookie.setCookie('token', '')\n            }\n            if (res.data.data.orgRoot.org_name) {\n              locCookie.setCookie('orgName', res.data.data.orgRoot.org_name)\n            } else {\n              locCookie.setCookie('orgName', '')\n            }\n\n            var option = {\n              token: res.data.data.token,\n              userId: res.data.data.userId,\n              seed: res.data.data.seed,\n              orgId: res.data.data.orgId,\n              username: res.data.data.user.user_name,\n              headimg: res.data.data.user.head_img,\n              orgName: res.data.data.orgRoot.org_name\n            }\n\n            this.$store.commit('SET_TOKEN_DATA', option)\n            this.$store.commit('SET_COMMENTS_ID', msgoption.id_item)\n            if (this.token) {\n              this.getRepo()\n              this.getMaterialData()\n              //this.getWaterMark()\n\n              this.getcontentinfo(msgoption)\n              this.getArticleDetail(msgoption)\n            }\n\n\n          } else {\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          console.error(error)\n        })\n      }\n\n    },\n    getParam(name, url) {\n      if (!url) url = location.href;\n      name = name.replace(/[\\[]/, \"\\\\\\[\").replace(/[\\]]/, \"\\\\\\]\");\n      var regexS = \"[\\\\?&]\" + name + \"=([^&#]*)\";\n      var regex = new RegExp(regexS);\n      var results = regex.exec(url);\n      return results == null ? null : results[1];\n    },\n\n    getUrlCode() {\n      let currentUrl = window.location.href\n      let token = this.getParam('token', currentUrl)\n      let msg_type = this.getParam('msg_type', currentUrl)\n      let id_item = this.getParam('id_item', currentUrl)\n      if (token) {\n        localStorage.setItem('token', token)\n      }\n      var option = {\n        token: token,\n        msg_type: msg_type,\n        id_item: id_item,\n        id_content: id_item\n      }\n      return option\n    },\n    getRepo() {\n      this.$store.commit('SET_CLEAR_REPODATA')\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数\n          \"flag_type\": 'article',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch('GET_REPOFODLER_DATA', option)\n    },\n\n    getMaterialData() {\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数\n          \"flag_type\": 'material',         // 必须字段：  库类型， 稿件库为 article，素材库为 material\n        }\n      }\n      this.$store.dispatch('GET_MATERIAL_DATA', option)\n    },\n\n    //获取水印列表\n    getWaterMark() {\n      var option = {\n        \"userId\": this.userId,        // 必须字段：用户 ID\n        \"orgId\": this.orgId,          // 必须字段：企业 ID\n        \"token\": this.token,          // 必须字段：token\n        \"seed\": this.seed,            // 必须字段：seed\n        \"params\": {                 // 必须字段： 参数\n        }\n      }\n      this.$store.dispatch('GET_WATERMARKLIST_DATA', option)\n\n    },\n    //获取文章详情\n    getArticleDetail(item) {\n      var option = {\n        \"userId\": this.userId,                  // 必须字段：用户 id\n        \"token\": this.token,                     // 必须字段： token\n        \"seed\": this.seed,                       // 必须字段： seed\n        'orgId': this.orgId,\n        \"params\": {                            // 必须字段： 创建文章参数\n          \"api_name\": \"get_by_article_id\",   // 必须固定值字段： 值： get_by_article_id\n          \"id\": item.id_item,               // 必须字段： 文章（稿件） id\n        }\n      }\n      api._post('article/get_article', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          if (res.data.data && res.data.data.total && res.data.data.total == 1) {\n            this.articleDetail = res.data.data.list[0]\n            this.$store.commit('SET_SHARE_ARTICLEDATA', res.data.data.list[0])\n            this.getAiAnylisData(res.data.data.list[0])\n            this.getReportNewsData(res.data.data.list[0])\n          } else {\n            this.articleDetail = ''\n            this.$store.commit('SET_SHARE_ARTICLEDATA', '')\n          }\n\n        } else {\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        console.error(error)\n      })\n    },\n\n    //AI详情分析\n    getAiAnylisData(item) {\n      //AI分析\n      var aiOption = {\n        \"userId\": this.userId,             // 必须字段：用户 id\n        \"token\": this.token,                // 必须字段： token\n        \"seed\": this.seed,\n        'orgId': this.orgId,\n        \"params\": {                       // 必须字段： 参数\n          \"content\": item.text,    // 必须字段： 文章内容\n          \"id\": \"\",               // 可选字段: 文章ID\n          \"title\": \"\"         // 可选字段： 文章标题\n        }\n      }\n      this.$store.dispatch('GET_ANALYSIS_DATA', aiOption)\n    },\n\n    getReportNewsData(item) {\n      //获取报题信息\n      if (item.flag_sys_source == \"news_recover\") {\n        this.$store.commit('SET_NEWSTOPICINFO_DATA', '')\n        var newstopicOption = {\n          \"userId\": this.userId,              // 必须字段：用户 id\n          \"orgId\": this.orgId,                // 必须字段：org id\n          \"token\": this.token,                 // 必须字段： token\n          \"seed\": this.seed,                   // 必须字段： seed\n          \"params\": {                        // 必须字段： 创建收藏记录参数\n            \"type\": \"article\",     // 必须字段： 类型，文章：article  素材： material\n            \"id\": item.id,               // 必须字段： 文章 ID 或者 是素材 ID\n          }\n        }\n        this.$store.dispatch('GET_NEWSTOPIC_INFODATA', newstopicOption)\n\n      }\n\n    },\n    goToIndex() {\n      this.$router.replace('/collectandsend/collecentry')\n      this.$store.commit('SET_SAVE_CURRENTROUTER', this.$route.path)\n      this.$store.commit('SET_ISPOSITIONMYSHARE_STATUS', true)\n    },\n    closecommentsbox() {\n      this.$root.eventHub.$emit('closeCommens', '')\n    }\n\n\n\n  },\n\n  watch: {\n\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n* {\n  box-sizing: border-box;\n}\nul,\nli {\n  list-style: none;\n}\n.audit {\n  width: 100%;\n  height: 100%;\n  background: #f0f3fa;\n}\n.audit_top {\n  width: 100%;\n  height: 70px;\n  background-color: rgba(255, 255, 255, 1);\n  box-shadow: 1.5px 2.6px 7px 0px rgba(0, 0, 0, 0.07);\n  border: 1px solid rgba(219, 224, 236, 1);\n  line-height: 70px;\n  padding-left: 30px;\n  cursor: pointer;\n}\n.audit_top span {\n  color: #333333;\n  font-size: 14px;\n  font-family: \"微软雅黑\";\n}\n\n.audit_box {\n  width: 100%;\n  height: calc(100% - 70px);\n  overflow: auto;\n}\n.audit_box_content {\n  width: 68%;\n  height: 94%;\n  background: red;\n  margin: 20px auto;\n  background-color: rgba(255, 255, 255, 1);\n  box-shadow: 1.5px 2.6px 7px 0px rgba(0, 0, 0, 0.07);\n  border: 1px solid rgba(219, 224, 236, 1);\n}\n@media screen and (max-width: 1540px) {\n}\n</style>\n"],sourceRoot:""}])},Vivn:function(n,t,e){var i=e("SxKq");"string"==typeof i&&(i=[[n.i,i,""]]),i.locals&&(n.exports=i.locals);e("rjj0")("13dc4187",i,!1,{})},W7Ch:function(n,t){n.exports="data:image/png;base64,"},WKt0:function(n,t){n.exports="data:image/png;base64,"},WsTr:function(n,t,e){(n.exports=e("FZ+f")(!0)).push([n.i,'\n.modal[data-v-15f56d14] {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  z-index: 1000;\n}\n.fade-enter-active[data-v-15f56d14] {\n  -webkit-animation: fade-in-data-v-15f56d14 .3s;\n          animation: fade-in-data-v-15f56d14 .3s;\n}\n.fade-leave-active[data-v-15f56d14] {\n  -webkit-animation: fade-out-data-v-15f56d14 .3s;\n          animation: fade-out-data-v-15f56d14 .3s;\n}\n@-webkit-keyframes fade-in-data-v-15f56d14 {\n0% {\n    -webkit-transform: transl