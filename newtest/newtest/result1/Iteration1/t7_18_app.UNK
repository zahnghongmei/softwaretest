AAA;EACA,YAAA;CACA;AAEA;EACA,YAAA;EACA,aAAA;EACA,qBAAA;EAAA,qBAAA;EAAA,cAAA;EACA,yBAAA;MAAA,sBAAA;UAAA,wBAAA;EACA,0BAAA;MAAA,uBAAA;UAAA,oBAAA;CACA;AACA;EACA,aAAA;EACA,aAAA;EACA,mBAAA;EACA,oBAAA;EACA,kBAAA;EACA,mBAAA;EACA,YAAA;EACA,gBAAA;EACA,gBAAA;CACA;AACA;EACA,iBAAA;EACA,eAAA;EACA,0BAAA;EACA,kBAAA;CACA",file:"modalMember.vue",sourcesContent:[".modal {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n  z-index: 1000;\n}\n\n.fade-enter-active {\n  animation: fade-in .3s;\n}\n\n.fade-leave-active {\n  animation: fade-out .3s;\n}\n\n@keyframes fade-in {\n  0% {\n    transform: translate3d(0, -5px, 0);\n    opacity: 0;\n  }\n  100% {\n    transform: translate3d(0, 0, 0);\n    opacity: 1;\n  }\n}\n\n@keyframes fade-out {\n  0% {\n    transform: translate3d(0, 0, 0);\n    opacity: 1;\n  }\n  100% {\n    transform: translate3d(0, -5px, 0);\n    opacity: 0;\n  }\n}\n\n.close {\n  width: 32px;\n  height: 32px;\n  border-radius: 50%;\n  position: absolute;\n  right: -16px;\n  top: -16px;\n  background-color: #A6A6A6;\n  border: 4px solid #F8F8F8;\n  text-align: center;\n  line-height: 30px;\n  color: #FFFFFF;\n  cursor: pointer;\n}\n\n.close:hover {\n  background-color: #77A7FF;\n}","<template>\n  <transition name=\"fade\">\n    <div class=\"modal\"\n         v-show=\"show\">\n      <div class=\"container\"\n           @click.stop='showSearchUl=false'>\n        <div class=\"container_top\">\n          <span class=\"container_top_title\">分享至他人</span>\n          <span class=\"iconfont icon-close\"\n                v-on:click=\"closeModal()\"></span>\n        </div>\n        <div class=\"search_person\">\n          <input type=\"text\"\n                 placeholder='请输入姓名按回车键查询'\n                 @keyup.enter='searchPersonData()'\n                 v-model='searchPersonKey'\n                 @keyup.delete='cancleSearch()'\n                 @click.stop='getMoreUserData()'>\n          <img src=\"../../static/img/search.png\"\n               alt=\"\"\n               @click.stop='searchPersonData()'>\n          <div class=\"search_ul\"\n               v-show='showSearchUl'>\n            <ul class=\"search_ul_list\">\n              <li v-for='(item,index) in searchPersonList'\n                  @click.stop='addPerson(item)'>\n                <img :src=\"item.head_img\"\n                     alt=\"\"\n                     v-if='item.head_img'>\n                <img src=\"../../static/img/avatar.png\"\n                     alt=\"\"\n                     v-if='!item.head_img'>\n                <span>{{item.user_name}}</span>\n              </li>\n            </ul>\n\n          </div>\n        </div>\n        <div class=\"share_list\">\n          <div class=\"share_list_left\">\n            <div class=\"share_list_title\">组织或部门</div>\n            <ul class=\"share_list_content\">\n              <li v-for='(item,index) in depList'>\n                <div class=\"share_each\"\n                     @click.stop='getOrgChildPerson(item,index)'\n                     style=\"cursor:pointer;\">\n                  <span>\n                    <span class=\"iconfont icon-triangle-down\"\n                          v-if='isShowPerson[index]'></span>\n                    <span class=\"iconfont icon-triangle-right-big\"\n                          v-else></span>\n                  </span>\n                  <span class=\"sharch_name\">{{item.org_name}}</span>\n                  <div>\n                  </div>\n                </div>\n                <ul v-show='isShowPerson[index]'>\n                  <li v-for='(itemone,indexone) in item.person'\n                      @click.stop='choicePerson($event,itemone)'\n                      :class='\"person\"+itemone.id'\n                      style=\"cursor:pointer;\">\n                    <div class=\"share_each\">\n                      <img :src=\"itemone.head_img\"\n                           alt=\"\"\n                           v-if='itemone.head_img'>\n                      <img src=\"../../static/img/avatar.png\"\n                           alt=\"\"\n                           v-if='!itemone.head_img'>\n                      <span class=\"sharch_name sharch_name_second\">{{itemone.user_name}}</span>\n                      <div>\n                        <div class=\"share_choice\"></div>\n                        <div class=\"share_haschoice\">\n                          <span class=\"iconfont icon-check\"></span>\n                        </div>\n                      </div>\n                    </div>\n                  </li>\n                </ul>\n              </li>\n            </ul>\n          </div>\n          <div class=\"share_list_left share_list_right\">\n            <div class=\"share_list_title\">\n              <span>已选择人员</span>\n              <span style=\"color:#51BD90;\">{{userChoiceList.length}}</span>\n              <span>个</span>\n            </div>\n            <ul class=\"share_list_content\">\n              <li v-for='(item,index) in userChoiceList'>\n                <div class=\"share_each\">\n                  <span class=\"sharch_name\">{{item.name}}</span>\n                  <div>\n                    <div class=\"share_clear\"><span class=\"iconfont icon-close\"\n                            @click.stop='deleteChoicePerson(item,index)'></span></div>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n        <div class=\"share_button\">\n          <div class=\"sure_cancle\"\n               @click.stop='sureSubmitShare()'>确&nbsp;&nbsp;定</div>\n          <div class=\"sure_cancle cancle_operate\"\n               @click.stop='closeModal()'>取&nbsp;&nbsp;消</div>\n        </div>\n      </div>\n    </div>\n  </transition>\n</template>\n\n<script>\nimport * as api from '../../api'\n// var debounce = require('throttle-debounce/debounce')\nconst cookie = require('browser-cookies')\nimport { niceScroller } from '../../helpers/niceScroller'\nimport Vue from 'vue'\nexport default {\n\n  components: {\n\n  },\n\n  created() {\n\n\n  },\n\n  mounted() {\n    this.$root.eventHub.$on('setMemberModal', (obj, type) => {\n      this.show = true\n      this.shareData = obj\n      this.personType = type\n    })\n    niceScroller('.search_ul_list')\n\n  },\n\n  destroyed() {\n    this.$root.eventHub.$off('setMemberModal')\n  },\n\n  data() {\n    return {\n      show: false,\n      depList: [],\n      orgPersonList: [],\n      isShowPerson: [],\n      userChoiceList: [],\n      showSearchUl: false,\n      searchPersonKey: '',\n      searchPersonList: [],\n      shareData: '',\n      personType: ''\n    }\n  },\n\n  updated() {\n    if (this.scroller) {\n      return this.scroller.resize()\n    }\n    this.scroller = niceScroller('.share_list_content')\n    this.scroller = niceScroller('.search_ul_list')\n  },\n\n  computed: {\n    userId() {\n      return this.$store.state.userinfo.userId\n    },\n    token() {\n      return this.$store.state.userinfo.token\n    },\n    seed() {\n      return this.$store.state.userinfo.seed\n    },\n    orgId() {\n      return this.$store.state.userinfo.orgId\n    },\n    shareType() {\n      return this.$store.state.articlelist.shareType\n    }\n\n\n  },\n\n  methods: {\n    getDepData() {\n      var option = {\n        \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n        \"token\": this.token,    // 必须字段：  token\n        \"seed\": this.seed,      // 必须字段：  seed\n        'orgId': this.orgId,\n        \"params\": {          // 必须字段：  参数，组织单位信息\n\n        }\n      }\n      api._post('org/get_org_info', option).then((res) => {\n        if (res.data && res.data.code && res.data.code == 200) {\n          this.depList = res.data.data || []\n          for (var i = 0; i < this.depList.length; i++) {\n            Vue.set(this.depList[i], \"person\", [])\n          }\n\n        } else {\n          this.depList = []\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        this.depList = []\n        console.error(error)\n      })\n    },\n    getOrgChildPerson(item, index) {\n      if (this.isShowPerson[index] == true) {\n        this.$set(this.isShowPerson, index, false)\n      } else {\n        this.$set(this.isShowPerson, index, true)\n        var option = {\n          \"userId\": this.userId,  // 必须字段：  用户 id，必须字段\n          \"token\": this.token,    // 必须字段：  token\n          \"seed\": this.seed,      // 必须字段：  seed\n          'orgId': this.orgId,\n          \"params\": {          // 必须字段：  参数，组织单位信息\n            \"id\": item.id\n          }\n        }\n        api._post('user/get_by_id_sub_org', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            this.orgPersonList = res.data.data || []\n            this.depList[index].person = this.orgPersonList\n            this.$nextTick(() => {\n              for (var i = 0; i < this.orgPersonList.length; i++) {\n                for (var j = 0; j < this.userChoiceList.length; j++) {\n                  if (this.orgPersonList[i].id == this.userChoiceList[j].id) {\n                    var id = '.' + 'person' + this.orgPersonList[i].id\n                    $(id).find('.share_choice').css('display', 'none')\n                    $(id).find('.share_haschoice').css('display', 'block')\n                  }\n                }\n              }\n\n            })\n\n          } else {\n            this.orgPersonList = []\n            this.depList[index].person = []\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          this.orgPersonList = []\n          this.depList[index].person = []\n          console.error(error)\n        })\n      }\n\n    },\n    choicePerson(event, item) {\n      // var id = event.currentTarget 'person' + item.id\n      var id = '.' + 'person' + item.id\n      if ($(id).find('.share_choice').css('display') == 'block' && $(id).find('.share_haschoice').css('display') == 'none') {\n        var k = 0;\n        for (var i = 0; i < this.userChoiceList.length; i++) {\n          if (this.userChoiceList[i].id != item.id) {\n            k++;\n          } else {\n            break;\n          }\n\n        }\n        if (k == this.userChoiceList.length) {\n          $(id).find('.share_choice').css('display', 'none')\n          $(id).find('.share_haschoice').css('display', 'block')\n          this.userChoiceList.push({ name: item.user_name, id: item.id })\n          var cls = '.' + 'person' + item.id\n          $(cls).find('.share_choice').css('display', 'none')\n          $(cls).find('.share_haschoice').css('display', 'block')\n        } else {\n          toastr.info('列表已经存在该人员')\n        }\n\n\n      } else {\n        $(id).find('.share_choice').css('display', 'block')\n        $(id).find('.share_haschoice').css('display', 'none')\n        var cls = '.' + 'person' + item.id\n        $(cls).find('.share_choice').css('display', 'block')\n        $(cls).find('.share_haschoice').css('display', 'none')\n        for (var j = 0; j < this.userChoiceList.length; j++) {\n          if (this.userChoiceList[j].id == item.id) {\n            this.userChoiceList.splice(j, 1)\n          }\n        }\n      }\n\n    },\n    deleteChoicePerson(item, index) {\n      var id = '.' + 'person' + item.id\n      $(id).find('.share_choice').css('display', 'block')\n      $(id).find('.share_haschoice').css('display', 'none')\n      this.userChoiceList.splice(index, 1)\n    },\n    searchPersonData() {\n      if (this.searchPersonKey != '') {\n        this.showSearchUl = true\n        var option = {\n          \"userId\": this.userId,         // 必须字段：  用户 id，必须字段\n          \"token\": this.token,          // 必须字段：  token\n          \"seed\": this.seed,            // 必须字段：  seed\n          'orgId': this.orgId,\n    