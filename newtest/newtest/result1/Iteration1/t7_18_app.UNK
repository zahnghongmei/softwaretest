is.$store.state.articlelist.downWhichArticleLsit\n    }\n  },\n  methods: {\n    getArticleDetail(item, index) {\n      this.$store.commit(\'SET_ARTICLE_DETAIL\', item)\n      this.currentIndex = index\n      //获取文章详情\n      var option = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 参数\n          "content": item.text,    // 必须字段： 文章内容\n          "id": "",               // 可选字段: 文章ID\n          "title": ""         // 可选字段： 文章标题\n        }\n      }\n      this.$store.dispatch(\'GET_ANALYSIS_DATA\', option)\n      this.$store.commit(\'SET_COMMENTS_ID\', item.id)\n      this.getAllArticleDataInfo(item)\n\n    },\n    editorArticle(item) {\n      this.$store.commit(\'SET_EDITORDATA_ITEM\', item)\n      this.$store.commit(\'SET_COMMENTS_ID\', item.id)\n      this.getAllArticleDataInfo(item)\n      this.$store.commit(\'SET_SAVE_CURRENTROUTER\', this.$route.path)\n      this.$router.replace(\'/editormain/editorcontent\')\n      this.$store.commit(\'SET_EDITORBUTTON_ROLE\', true)\n\n    },\n    getAllArticleDataInfo(item) {\n      //获取评论\n      var comments = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n          "page_no": 0,                  // 可选字段： 分页，页号，从 0 开始， 默认 0\n          "page_size": 1000               // 可选字段： 分页，返回数量，默认 10，最大 100\n        }\n      }\n      this.$store.dispatch(\'GET_ARTICLE_COMMENTS\', comments)\n      //获取日志\n      var logoption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n        }\n      }\n\n      this.$store.dispatch(\'GET_LOG_OPERATEDATA\', logoption)\n      //获取历史记录也就是留痕\n      var historyOption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                       // 必须字段： 文章参数\n          "id": item.id,           // 必须字段： 文章 ID\n          "page_no": 0,                  // 可选字段： 分页，页号，从 0 开始， 默认 0\n          "page_size": 1000               // 可选字段： 分页，返回数量，默认 10，最大 100\n        }\n      }\n      this.$store.dispatch(\'GET_HISTORY_RECORD\', historyOption)\n\n      //获取审核日志\n      var auditLogOption = {\n        "userId": this.userId,             // 必须字段：用户 id\n        "orgId": this.orgId,               // 必须字段：公司 id\n        "token": this.token,                // 必须字段： token\n        "seed": this.seed,                  // 必须字段： seed\n        "params": {                       // 必须字段： 参数\n          "id_content": item.id,        // 必须字段：审核内容的 id（稿件 ID 或 视频 ID）\n          "item_type": "article",    // 必须字段：类型  稿件为  article  视频为 material\n          "page_no": 0,                      // 可选字段：分页参数，页号，从 0 开始\n          "page_size": 1000                   // 可选字段：分页参数，每页长度，默认为 100\n        }\n      }\n      this.$store.dispatch(\'GET_AUDIT_LOGLIST\', auditLogOption)\n\n      //获取报题信息\n      if (item.flag_sys_source == "news_recover") {\n        this.$store.commit(\'SET_NEWSTOPICINFO_DATA\', \'\')\n        var newstopicOption = {\n          "userId": this.userId,              // 必须字段：用户 id\n          "orgId": this.orgId,                // 必须字段：org id\n          "token": this.token,                 // 必须字段： token\n          "seed": this.seed,                   // 必须字段： seed\n          "params": {                        // 必须字段： 创建收藏记录参数\n            "type": "article",     // 必须字段： 类型，文章：article  素材： material\n            "id": item.id,               // 必须字段： 文章 ID 或者 是素材 ID\n          }\n        }\n        this.$store.dispatch(\'GET_NEWSTOPIC_INFODATA\', newstopicOption)\n\n      }\n\n\n\n\n    },\n\n\n    collectCurrentArticle(item, index) {\n      var option = {\n        "userId": this.userId,              // 必须字段：用户 id\n        "token": this.token,                 // 必须字段： token\n        "seed": this.seed,                   // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                        // 必须字段： 创建收藏记录参数\n          "id": item.id       // 必须字段： 文章 ID\n        }\n      }\n      api._post(\'article_collection/add_collection\', option).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          toastr.success(\'收藏成功！\')\n          item.is_collection = true\n        } else {\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n        }\n      }).catch((error) => {\n        toastr.error(error)\n      })\n\n    },\n    cancleCollectArticle(item, index) {\n      var option = {\n        "userId": this.userId,              // 必须字段：用户 id\n        "token": this.token,                 // 必须字段： token\n        "seed": this.seed,                   // 必须字段： seed\n        \'orgId\': this.orgId,\n        "params": {                        // 必须字段： 创建收藏记录参数\n          "id": item.id       // 必须字段： 文章 ID\n        }\n      }\n      api._post(\'article_collection/delete_collection\', option).then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          toastr.success(\'取消收藏成功！\')\n          item.is_collection = false\n        } else {\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n        }\n      }).catch((error) => {\n        toastr.error(error)\n      })\n    },\n    shareToOther(item) {\n      this.$root.eventHub.$emit(\'setMemberModal\', item, "sharearticle")\n    },\n    deleteArticle(item) {\n      this.$store.commit(\'SET_DELETE_TYPE\', \'deleteArticle\')\n      this.$root.eventHub.$emit(\'setDeteModel\', item)\n    },\n    saveAsOther(item) {\n      this.$root.eventHub.$emit(\'setMSaveAs\', item, \'manuscript\')\n    },\n    pushArticle(item) {\n      //this.$root.eventHub.$emit(\'setPushModel\', item)\n      this.$root.eventHub.$emit(\'setPushService\', item)\n      //this.$root.eventHub.$emit(\'setSimulationPushModel\', \'\')\n    }\n\n\n\n\n  },\n  watch: {\n    dataList() {\n      this.currentIndex = 0\n      if (this.dataList && this.dataList.length > 0) {\n        this.$store.commit(\'SET_ARTICLE_DETAIL\', this.dataList[0])\n        var option = {\n          "userId": this.userId,             // 必须字段：用户 id\n          "token": this.token,                // 必须字段： token\n          "seed": this.seed,\n          \'orgId\': this.orgId,\n          "params": {                       // 必须字段： 参数\n            "content": this.dataList[0].text,    // 必须字段： 文章内容\n            "id": "",               // 可选字段: 文章ID\n            "title": ""         // 可选字段： 文章标题\n          }\n        }\n        this.$store.dispatch(\'GET_ANALYSIS_DATA\', option)\n        this.$store.commit(\'SET_COMMENTS_ID\', this.dataList[0].id)\n        this.getAllArticleDataInfo(this.dataList[0])\n\n      } else {\n        this.$store.commit(\'SET_ARTICLE_DETAIL\', \'\')\n        this.$store.commit(\'SET_CLEARAI_DATA\')\n        this.$store.commit(\'SET_CLEARAI_COMMENTS\')\n        this.$store.commit(\'SET_COMMENTS_ID\', \'\')\n        this.$store.commit(\'SET_CLEARAI_LOGOPERATE\')\n        this.$store.commit(\'SET_CLEARAI_HISTORYRECORD\')\n        this.$store.commit(\'SET_CLEARAI_AUDITLOG\')\n        this.$store.commit(\'SET_NEWSTOPICINFO_DATA\', \'\')\n      }\n    }\n  }\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only -