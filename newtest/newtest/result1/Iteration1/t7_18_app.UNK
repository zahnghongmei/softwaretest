  "params": {                         // 必须字段： 请求参数\n          "userName": this.userName,      // 必须字段： 北方网用户名\n          "password": this.password      // 必须字段：  用户密码\n        }\n      }\n      if (this.userName && this.password) {\n        api._post(\'pusher/gog_north_net/login\', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            toastr.success(\'登录成功\')\n            this.isvisible = false\n          }\n          else {\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          console.error(error)\n        })\n\n      } else {\n        toastr.info(\'请填写完用户名和密码\')\n      }\n\n\n    }\n\n  },\n  watch: {\n    isvisible() {\n      if (this.isvisible == false) {\n        this.userName = \'\'\n        this.password = \'\'\n      }\n    }\n  }\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only --\x3e\n<style scoped>\n@import "../../static/css/modal.css";\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n.editorprocesstor {\n  width: 600px;\n  margin: 100px auto;\n  background: #ffffff;\n  border-radius: 0 0 10px 10px;\n}\n.container_top {\n  width: 100%;\n  height: 44px;\n  background: #f7faff;\n  position: relative;\n  padding-left: 20px;\n  padding-right: 20px;\n  clear: both;\n  line-height: 44px;\n  border-bottom: 1px solid #c0beff;\n  margin-bottom: 20px;\n}\n.container_top .iconfont {\n  float: right;\n  font-size: 20px;\n  cursor: pointer;\n  color: #5f7094;\n  line-height: 44px;\n}\n.container_top .container_top_title {\n  color: #333333;\n  font-size: 14px;\n  font-family: "微软雅黑";\n  float: left;\n}\n.editor_content {\n  width: 100%;\n  height: 140px;\n  background: #ffffff;\n}\n.editor_each_item {\n  display: flex;\n  padding-right: 20px;\n  margin-bottom: 20px;\n}\n.editor_each_title {\n  width: 150px;\n  clear: both;\n}\n.editor_each_title span {\n  float: right;\n  color: #474747;\n  font-size: 16px;\n  line-height: 40px;\n}\n\n.editor_each_item input {\n  border: 1px solid #cad0e0;\n  border-radius: 6px;\n  padding-left: 10px;\n  width: 300px;\n  height: 38px;\n  color: #999999;\n  font-size: 14px;\n  background: #fff;\n  margin-left: 30px;\n  outline: none;\n}\n.custom_add {\n  width: 300px;\n  height: 40px;\n  background: rgba(0, 206, 159, 1);\n  margin-left: 50px;\n  color: #fff;\n  font-size: 14px;\n  text-align: center;\n  line-height: 40px;\n  cursor: pointer;\n  border-radius: 20px;\n}\n/* .editor_each_input input[type="text"]:webkit-autofill,\n.editor_each_input input[type="password"]:webkit-autofill {\n  -webkit-box-shadow: 0 0 1000px #fff inset !important;\n  -webkit-text-fill-color: rgba(255, 255, 255, 1) !important;\n} */\n</style>\n'],sourceRoot:""}])},zeXe:function(n,t){n.exports="data:image/png;base64,"},zp2c:function(n,t,e){var i=e("5y9o");"string"==typeof i&&(i=[[n.i,i,""]]),i.locals&&(n.exports=i.locals);e("rjj0")("11416bca",i,!1,{})}},[0]);
