ole.error(error)\n        })\n\n      } else if (this.deleteType == 'deleteArticle') {\n        var option = {\n          \"userId\": this.userId,             // 必须字段：用户 id\n          \"token\": this.token,                // 必须字段： token\n          \"seed\": this.seed,                  // 必须字段： seed\n          'orgId': this.orgId,\n          \"params\": {                       // 必须字段： 文章参数\n            \"id\": this.deleteObj.id,           // 必须字段： 文章 ID\n          }\n        }\n        api._post('article/delete_article', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            toastr.success('删除成功')\n            this.$store.commit('SET_DELETEARTICLE_LIST', this.deleteObj.id)\n            var getTypeData = ''\n            var ids = ''\n            if (this.downWhichArticleLsit == 'normal') {\n              getTypeData = 'by_id_folder'\n              ids = this.editorFoldId\n            } else if (this.downWhichArticleLsit == 'collect') {\n              getTypeData = 'by_article_collection'\n              ids = ''\n            } else if (this.downWhichArticleLsit == 'mesharetoother') {\n              getTypeData = 'by_share'\n              ids = ''\n            } else if (this.downWhichArticleLsit == 'sharetome') {\n              getTypeData = 'by_be_share'\n              ids = ''\n            }\n            var sendCurrentData = {\n              \"userId\": this.userId,\n              \"token\": this.token,\n              \"seed\": this.seed,\n              'orgId': this.orgId,\n              \"params\": {\n                \"api_name\": getTypeData,\n                \"id\": ids\n              }\n            }\n            if (this.$route.path != '/searcharticle/searcharticlelist') {\n              this.$store.dispatch('GET_ARTICLE_ALLTOTALANDUPDATE', sendCurrentData)\n            }\n            if (this.$route.path == '/searcharticle/searcharticlelist') {\n              this.$store.commit('SET_SEARCHTOTAAL_DELETE')\n   