\n          this.pushServiceList = res.data.data\n        } else {\n          this.pushServiceList = []\n          if (res.data && res.data.message) {\n            toastr.warning(res.data.message)\n          }\n        }\n\n      }).catch((error) => {\n        this.pushServiceList = []\n        console.error(error)\n      })\n    },\n    pushEveryEndPoInt(item) {\n\n      if (item.id == 'id_gsh_app') {\n        this.$root.eventHub.$emit('setPushGuanshanappInfo', this.pushObj)\n      }\n\n      if (item.id == 'id_zhongwang_app') {\n        this.$root.eventHub.$emit('setPushZhongWangApp', this.pushObj)\n\n      }\n\n      if (item.id == 'id_gognorthnet') {\n        this.$root.eventHub.$emit('setPushPcInfo', this.pushObj)\n      }\n      if (item.id == 'id_sinan_app') {\n        this.$root.eventHub.$emit('setPushSiNanAppInfo', this.pushObj)\n      }\n      if (item.id == 'id_gsh_micro_site') {\n        this.$root.eventHub.$emit('setWeiZhanPushInfo', this.pushObj)\n      }\n      if (item.id == 'id_bh_cms_site') {\n        this.$root.eventHub.$emit('setCmsSitePushInfo', this.pushObj)\n      }\n      if (item.id == 'id_pingtang_site') {\n        this.$root.eventHub.$emit('pingTangPushInfo', this.pushObj)\n      }\n\n\n\n    }\n\n\n\n  },\n\n  watch: {\n    show() {\n      if (this.show == false) {\n        this.pushObj = ''\n        this.pushServiceList = []\n      } else {\n        this.getPushServiceList()\n      }\n    }\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n@import \"../../static/css/modal\";\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nul,\nli {\n  list-style: none;\n}\n\n.expand-transition {\n  transition: all 1s ease;\n}\n\n.expand-enter,\n.expand-leave {\n  opacity: 0;\n}\n\n.expand-enter .container,\n.expand-leave .container {\n  /*margin-top: -800px;*/\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n\n.container {\n  transition: all 1s ease;\n  width: 930px;\n  height: 750px;\n  position: fixed;\n  left: 25%;\n  top: