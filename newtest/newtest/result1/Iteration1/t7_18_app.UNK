return string.split(\'\\s+\')\n    },\n\n    closeUploadpic() {\n      this.show = false\n    },\n    sureSubmit() {\n      this.modifyData.tags = this.stringToArray(this.modifyData.tags)\n      this.modifyData.keywords = this.stringToArray(this.modifyData.keywords)\n      console.log(this.modifyData)\n      var option = {\n        "userId": this.userId,        // 必须字段：用户 ID\n        "orgId": this.orgId,          // 必须字段：企业 ID\n        "token": this.token,          // 必须字段：token\n        "seed": this.seed,            // 必须字段：seed\n        "params": this.modifyData\n      }\n      if (this.modifyType == "picture") {\n        api._post(\'material/picture/picture_update\', option).then((resp) => {\n          if (resp.data && resp.data.code && resp.data.code == 200) {\n            toastr.success(\'修改成功\')\n            this.show = false\n            this.$store.commit(\'SET_MODIFT_PIC\', this.modifyData)\n          } else {\n            if (resp.data && resp.data.message) {\n              toastr.warning(resp.data.message)\n            }\n          }\n        }).catch((error) => {\n          toastr.error(error)\n        })\n\n      }\n      else if (this.modifyType == "project") {\n        api._post(\'material/picture/topic_update\', option).then((resp) => {\n          if (resp.data && resp.data.code && resp.data.code == 200) {\n            toastr.success(\'修改成功\')\n            this.show = false\n            this.$store.commit(\'SET_MODIFT_PIC\', this.modifyData)\n          } else {\n            if (resp.data && resp.data.message) {\n              toastr.warning(resp.data.message)\n            }\n          }\n        }).catch((error) => {\n          toastr.error(error)\n        })\n\n      }\n      else if (this.modifyType == "video") {\n        api._post(\'material/video/video_update\', option).then((resp) => {\n          if (resp.data && resp.data.code && resp.data.code == 200) {\n            toastr.success(\'修改成功\')\n            this.show = false\n            this.$store.commit(\'SET_VIDEOSOMEONE_DATA\', this.modifyData)\n            this.$store.commit(\'SET_VIDEOEACHINFO_DATA\', this.modifyData)\n          } else {\n            if (resp.data && resp.data.message) {\n              toastr.warning(resp.data.message)\n            }\n          }\n        }).catch((error) => {\n          toastr.error(error)\n        })\n      }\n\n    },\n\n    getTopicProjectList() {\n      api._post(\'material/picture/get_topic/\' + this.modifyData.id_folder, \'\').then((resp) => {\n        if (resp.data && resp.data.code && resp.data.code == 200) {\n          this.ownershipProjectList = resp.data.data\n          for (var i = 0; i < resp.data.data.length; i++) {\n            if (resp.data.data[i].id == this.modifyData.id_topic) {\n              this.topicName = resp.data.data[i].title\n              break;\n            }\n          }\n\n        } else {\n          if (resp.data && resp.data.message) {\n            toastr.warning(resp.data.message)\n          }\n        }\n      }).catch((error) => {\n        toastr.error(error)\n      })\n    },\n\n\n    showAttributionStatus() {\n      this.showAttributionProject = !this.showAttributionProject\n    },\n\n\n    choiceCurrentTopic(item) {\n      this.topicName = item.title\n      this.showAttributionProject = false\n      this.modifyData.id_topic = item.id\n    }\n\n\n  },\n  watch: {\n    show() {\n      if (this.show == false) {\n        this.clearboxData()\n      }\n    }\n  }\n}\n\n\n\n<\/script>\n\n\x3c!-- Add "scoped" attribute to limit CSS to this component only --\x3e\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nul,\nli {\n  list-style: none;\n}\n.upload_pic {\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.8);\n  position: fixed;\n  left: 0;\n  top: 0;\n  z-index: 100;\n}\n.upload_pic_box {\n  width: 550px;\n  height: 660px;\n  background: #fff;\n  border-radius: 4px;\n  margin: 100px auto;\n  padding-bottom: 20px;\n}\n.upload_pic_top {\n  