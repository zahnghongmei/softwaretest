nel\": this.sendPushData\n        }\n      }\n      if (this.sendPushData.length > 0) {\n        loadingTranstion()\n        api._post('pusher/gog_north_net/pusher', option).then((res) => {\n          if (res.data && res.data.code && res.data.code == 200) {\n            toastr.success('推送成功')\n            closeLoading()\n            this.show = false\n          } else if (res.data && res.data.code && res.data.code == -1) {\n            this.$root.eventHub.$emit('reloginmodal', '')\n            closeLoading()\n          }\n          else {\n            closeLoading()\n            if (res.data && res.data.message) {\n              toastr.warning(res.data.message)\n            }\n          }\n\n        }).catch((error) => {\n          closeLoading()\n          console.error(error)\n        })\n\n      } else {\n        toastr.warning('请至少选择一个栏目')\n      }\n\n    }\n\n  },\n\n  watch: {\n    show() {\n      if (this.show == false) {\n        this.sendPushData = []\n        this.pushObj = ''\n        $('.share_choicepush').css('display', 'block')\n        $('.share_haschoicepush').css('display', 'none')\n        this.$root.eventHub.$emit('clearpushdata', '')\n        this.columnTemplate = []\n        this.templateObj = ''\n        this.dataList = []\n        this.channelKeyWords = ''\n        this.templateKeyWords = ''\n        $('.container_send_each').removeClass('container_send_eachActive')\n        $('.container_send_each').eq(0).addClass('container_send_eachActive')\n\n      } else {\n        this.getColumnRootData()\n      }\n    }\n\n  }\n}\n\n<\/script>\n\n<style scoped>\n@import \"../../static/css/modal\";\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nul,\nli {\n  list-style: none;\n}\n\n.expand-transition {\n  transition: all 1s ease;\n}\n\n.expand-enter,\n.expand-leave {\n  opacity: 0;\n}\n\n.expand-enter .container,\n.expand-leave .container {\n  /*margin-top: -800px;*/\n  -webkit-transform: scale(1.1);\n  transform: scale(1.1);\n}\n\n.